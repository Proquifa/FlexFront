<?xml version="1.0" encoding="utf-8"?>
<s:CheckBox xmlns:fx="http://ns.adobe.com/mxml/2009" 
			xmlns:s="library://ns.adobe.com/flex/spark" 
			xmlns:mx="library://ns.adobe.com/flex/mx"
			styleName="{styleCheck}" >
	
	
	<fx:Script>
		<![CDATA[
			
			
			private var _styleCheck:String = "checkBoxBlancoAzul";
			
			[Bindable]
			[Inspectable(category="Styles", enumeration="checkBoxBlancoAzul,checkBoxBlancoVerde,checkBoxBlancoCuadroGrisPalomaVerde")]
			public function get styleCheck():String
			{
				return _styleCheck;
			}

			public function set styleCheck(value:String):void
			{
				_styleCheck = value;
			}

		]]>
	</fx:Script>
</s:CheckBox>
