<?xml version="1.0" encoding="utf-8"?>
<s:Group xmlns:fx="http://ns.adobe.com/mxml/2009" 
		 xmlns:s="library://ns.adobe.com/flex/spark" 
		 xmlns:mx="library://ns.adobe.com/flex/mx"
		 color="#FFFFFF" width="100%" height="3">
	<s:Rect x="0" y="0" width="100%" height="3" >
		<s:fill>
			<s:SolidColor color="{ getStyle( 'color' ) }" />
		</s:fill>
		<s:filters>
			<s:DropShadowFilter alpha="0.75" angle="45.0" blurX="1" blurY="1" color="#000000"
								distance="1" hideObject="false" inner="true" knockout="false"
								quality="2" strength="1"/>
		</s:filters>
	</s:Rect>
</s:Group>
