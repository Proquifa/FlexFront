<?xml version="1.0" encoding="utf-8"?>
<mx:VBox xmlns:fx="http://ns.adobe.com/mxml/2009" 
		 xmlns:s="library://ns.adobe.com/flex/spark" 
		 xmlns:mx="library://ns.adobe.com/flex/mx" width="100%" height="100" textAlign="center">
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	
	<fx:Script>
		<![CDATA[
			import skins.catalogos.catalogoColores;
			
			override public function set data(value:Object):void{
				super.data = value;
				
			}
		]]>
	</fx:Script>
	<s:Label  width="100%" height="100%" text="{this.data.finArray} {this.data.finArray == 1 ?  ' Permiso' : ' Permisos' }"   fontSize="12" fontStyle="normal" verticalAlign="bottom" textAlign="center" />
