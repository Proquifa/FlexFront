package mx.com.proquifa.proquifanet.rsl.vista.modelo.comun
{
	[RemoteClass(alias="mx.com.proquifa.proquifanet.modelo.comun.ValorAdicional")]
	[Bindable]
	public class ValorAdicional
	{
		
		private var _id:Number;
		private var _nombre:String;
		private var _valor:String;
		private var _valorSecundario:String;
		private var _tipo:String;
		private var _valorMostrado:String
		
		
		
		public function ValorAdicional()
		{
		}

		public function get id():Number
		{
			return _id;
		}

		public function set id(value:Number):void
		{
			_id = value;
		}

		public function get nombre():String
		{
			return _nombre;
		}

		public function set nombre(value:String):void
		{
			_nombre = value;
			_valorMostrado = _nombre;
		}

		public function get valor():String
		{
			return _valor;
		}

		public function set valor(value:String):void
		{
			_valor = value;
		}

		public function get valorSecundario():String
		{
			return _valorSecundario;
		}

		public function set valorSecundario(value:String):void
		{
			_valorSecundario = value;
		}

		public function get tipo():String
		{
			return _tipo;
		}

		public function set tipo(value:String):void
		{
			_tipo = value;
		}

		public function get valorMostrado():String
		{
			return _valorMostrado;
		}

		public function set valorMostrado(value:String):void
		{
			_valorMostrado = value;
		}


	}
}