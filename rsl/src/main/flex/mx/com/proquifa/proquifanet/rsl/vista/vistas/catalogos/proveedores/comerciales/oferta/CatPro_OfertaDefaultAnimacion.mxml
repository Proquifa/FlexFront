			protected function fadeIn( $target:Object ):void
			{
				var fade:Fade = new Fade();
				fade.target = $target;
				fade.alphaFrom = ($target as Image).alpha;
				fade.duration = 200;
				fade.alphaTo = 1;
				fade.play();
			}
			
			protected function fadeOut( $target:Object ):void
			{
				var fade:Fade = new Fade();
				fade.target = $target;
				fade.duration = 200;
				fade.alphaFrom = 1;
