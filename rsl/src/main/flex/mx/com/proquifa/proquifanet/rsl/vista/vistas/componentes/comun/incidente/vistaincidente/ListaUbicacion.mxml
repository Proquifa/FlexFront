<?xml version="1.0" encoding="utf-8"?>
<s:ItemRenderer xmlns:fx="http://ns.adobe.com/mxml/2009" 
				xmlns:s="library://ns.adobe.com/flex/spark" 
				xmlns:mx="library://ns.adobe.com/flex/mx" width="100%" height="100%" xmlns:label="skins.skin.label.*" xmlns:otros="skins.otros.*">
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	<fx:Script>
		<![CDATA[
			
			override public function set data(value:Object):void{
				super.data = value;
			}
			
		]]>
	</fx:Script>
	
	<s:VGroup height="100%" width="100%">
	<s:FormItem width="100%">
			<s:Label id="ubicacion" text="{data.origen}" height="100%" width="100%" textAlign="left" fontWeight="bold" fontFamily="Helvetica" fontSize="12"/>
		
			<s:Label id="usuario" text="{data.nombreResponsable}" width="100%" height="100%" color="#999999" textAlign="left" fontFamily="Helvetica" fontWeight="normal" fontSize="12"/>		
	</s:FormItem>
		<otros:division id="linea1"/>
	</s:VGroup>
</s:ItemRenderer>
