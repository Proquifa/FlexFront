			private function radiansToDegrees(radians:Number):Number {
				return radians * (180 / Math.PI);
			}
			
			public static function cuentaPalabras(txt:String):uint {
				var cuenta:uint = 0;
				if (txt != null) {
					//txt = StringUtil.trim(txt);
					if (txt.length > 0 ) {
						cuenta = txt.split(/\s+/g).length;
					}	
				}
				return cuenta;
			}
			
