package mx.com.proquifa.proquifanet.rsl.vista.modelo.comun
{
	import mx.collections.ArrayCollection;

	[RemoteClass(alias="mx.com.proquifa.proquifanet.modelo.comun.Parametros")]
	[Bindable]
	public class Parametros
	{
		private var _finicio:Date;
		private var _ffin:Date;
		private var _empresa:String;
		private var _referencia:String;
		private var _destinatario:String;
		private var _tipo:String;
		private var _abiertoCerrado:String;
		private var _niveles:ArrayCollection;
		private var _cPago:String;
		
		/*
		private List<NivelIngreso> niveles;		
		*/
		public function Parametros()
		{
		}
		
		
		public function get finicio():Date
		{
			return _finicio;
		}

		public function set finicio(value:Date):void
		{
			_finicio = value;
		}

		public function get ffin():Date
		{
			return _ffin;
		}

		public function set ffin(value:Date):void
		{
			_ffin = value;
		}

		public function get empresa():String
		{
			return _empresa;
		}

		public function set empresa(value:String):void
		{
			_empresa = value;
		}

		public function get referencia():String
		{
			return _referencia;
		}

		public function set referencia(value:String):void
		{
			_referencia = value;
		}

		public function get destinatario():String
		{
			return _destinatario;
		}

		public function set destinatario(value:String):void
		{
			_destinatario = value;
		}

		public function get tipo():String
		{
			return _tipo;
		}

		public function set tipo(value:String):void
		{
			_tipo = value;
		}

		public function get abiertoCerrado():String
		{
			return _abiertoCerrado;
		}

		public function set abiertoCerrado(value:String):void
		{
			_abiertoCerrado = value;
		}
		
		public function get niveles():ArrayCollection
		{
			return _niveles;
		}
		
		public function set niveles(value:ArrayCollection):void
		{
			_niveles = value;
		}

		public function get cPago():String
		{
			return _cPago;
		}

		public function set cPago(value:String):void
		{
			_cPago = value;
		}
	}
}