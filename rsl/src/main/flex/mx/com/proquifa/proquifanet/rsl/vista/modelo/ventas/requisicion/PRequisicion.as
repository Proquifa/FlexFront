package mx.com.proquifa.proquifanet.rsl.vista.modelo.ventas.requisicion
{
	
	[RemoteClass(alias="mx.com.proquifa.proquifanet.modelo.ventas.requisicion.PRequisicion")]
	[Bindable]
	public class PRequisicion
	{
		private var _numFila:int;
		private var _idPartida:int;
		private var _marca:String;
		private var _codigo:String;
		private var _concepto:String;
		private var _piezasACotizar:int;
		private var _cantidad:int;
		private var _unidad:String;
		private var _idDoctor:int;
		private var _precioU:Number;
		private var _moneda:String;
		private var _idVisita:Number;
		
		public function PRequisicion()
		{
		}

		public function get numFila():int
		{
			return _numFila;
		}

		public function set numFila(value:int):void
		{
			_numFila = value;
		}

		public function get moneda():String
		{
			return _moneda;
		}

		public function set moneda(value:String):void
		{
			_moneda = value;
		}

		public function get idPartida():int
		{
			return _idPartida;
		}

		public function set idPartida(value:int):void
		{
			
			_idPartida = value;
		}

		public function get marca():String
		{
			if(_marca == null)
				_marca = "";
			
			return _marca;
		}

		public function set marca(value:String):void
		{
			if(value == null)
				_marca = "";
			else
			_marca = value;
		}

		public function get codigo():String
		{
			return _codigo;
		}

		public function set codigo(value:String):void
		{
			_codigo = value;
		}

		public function get concepto():String
		{
			return _concepto;
		}

		public function set concepto(value:String):void
		{
			_concepto = value;
		}

		public function get piezasACotizar():int
		{
			return _piezasACotizar;
		}

		public function set piezasACotizar(value:int):void
		{
			_piezasACotizar = value;
		}

		public function get cantidad():int
		{
			return _cantidad;
		}

		public function set cantidad(value:int):void
		{
			_cantidad = value;
		}

		public function get unidad():String
		{
			return _unidad;
		}

		public function set unidad(value:String):void
		{
			_unidad = value;
		}

		public function get idDoctor():int
		{
			return _idDoctor;
		}

		public function set idDoctor(value:int):void
		{
			_idDoctor = value;
		}

		public function get precioU():Number
		{
			return _precioU;
		}

		public function set precioU(value:Number):void
		{
			_precioU = value;
		}

		public function get idVisita():Number
		{
			return _idVisita;
		}

		public function set idVisita(value:Number):void
		{
			_idVisita = value;
		}


	}
}