package mx.com.proquifa.proquifanet.rsl.vista.modelo.comun
{
	[RemoteClass(alias="mx.com.proquifa.proquifanet.modelo.comun.TopTen")]
	public class TopTen
	{
		private var _numeroFila:int;
		private var _cantidad:int;
		private var _monto:Number;
		
		private var _nombre:String;
		private var _formatoMonto:String;
		
		public function TopTen()
		{
		}
		
		public function get formatoMonto():String
		{
			return _formatoMonto;
		}
		
		public function set formatoMonto(value:String):void
		{
			_formatoMonto = value;
		}
		
		public function get numeroFila():int
		{
			return _numeroFila;
		}
		
		public function set numeroFila(value:int):void
		{
			_numeroFila = value;
		}
		
		public function get nombre():String
		{
			return _nombre;
		}
		
		public function set nombre(value:String):void
		{
			_nombre = value;
		}
		
		public function get monto():Number
		{
			return _monto;
		}
		
		public function set monto(value:Number):void
		{
			_monto = value;
		}
		
		public function get cantidad():int
		{
			return _cantidad;
		}
		
		public function set cantidad(value:int):void
		{
			_cantidad = value;
		}
		
	}
}