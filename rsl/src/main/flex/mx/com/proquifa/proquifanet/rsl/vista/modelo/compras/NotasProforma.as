package mx.com.proquifa.proquifanet.rsl.vista.modelo.compras
{
	[RemoteClass(alias="mx.com.proquifa.proquifanet.modelo.compras.tramitadorocphs.NotasProforma")]
	[Bindable]
	public class NotasProforma
	{
		private var _folio:Number;
		private var _totalNotas:int;
		private var _numfila:int;
		
		private var _fecha:Date;

		private var _eliminar:Boolean;
		private var _ultimaNota:Boolean;
		
		private var _nota:String;
		
		
		public function get numfila():int
		{
			return _numfila;
		}

		public function set numfila(value:int):void
		{
			_numfila = value;
		}

		public function get totalNotas():int
		{
			return _totalNotas;
		}

		public function set totalNotas(value:int):void
		{
			_totalNotas = value;
		}

		public function get ultimaNota():Boolean
		{
			return _ultimaNota;
		}

		public function set ultimaNota(value:Boolean):void
		{
			_ultimaNota = value;
		}

		public function get nota():String
		{
			return _nota;
		}

		public function set nota(value:String):void
		{
			_nota = value;
		}

		public function get eliminar():Boolean
		{
			return _eliminar;
		}

		public function set eliminar(value:Boolean):void
		{
			_eliminar = value;
		}

		public function get fecha():Date
		{
			return _fecha;
		}

		public function set fecha(value:Date):void
		{
			_fecha = value;
		}

		public function get folio():Number
		{
			return _folio;
		}

		public function set folio(value:Number):void
		{
			_folio = value;
		}

	}
}