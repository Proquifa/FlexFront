package mx.com.proquifa.proquifanet.rsl.vista.modelo.catalogos.proveedores
{
	import mx.collections.ArrayCollection;

	[RemoteClass(alias="mx.com.proquifa.proquifanet.modelo.catalogos.proveedores.Logistica")]
	[Bindable]
	public class Logistica
	{
		private var _idProveedor:Number;
		private var _tiempoEntrega:ArrayCollection;
		private var _flete:ArrayCollection;
		private var _fleteExpress:Flete;
		private var _fua:Date;
		
		public function Logistica()
		{
			flete = new ArrayCollection;
			tiempoEntrega = new ArrayCollection;
		}

		public function get idProveedor():Number
		{
			return _idProveedor;
		}

		public function set idProveedor(value:Number):void
		{
			_idProveedor = value;
		}

		public function get tiempoEntrega():ArrayCollection
		{
			return _tiempoEntrega;
		}

		public function set tiempoEntrega(value:ArrayCollection):void
		{
			_tiempoEntrega = value;
		}

		public function get flete():ArrayCollection
		{
			return _flete;
		}

		public function set flete(value:ArrayCollection):void
		{
			_flete = value;
		}

		public function get fua():Date
		{
			return _fua;
		}

		public function set fua(value:Date):void
		{
			_fua = value;
		}

		public function get fleteExpress():Flete
		{
			return _fleteExpress;
		}

		public function set fleteExpress(value:Flete):void
		{
			_fleteExpress = value;
		}




	}
}