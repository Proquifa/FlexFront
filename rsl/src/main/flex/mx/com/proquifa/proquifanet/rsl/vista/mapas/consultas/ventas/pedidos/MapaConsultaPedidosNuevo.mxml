				</mate:EventAnnouncer>
			</mate:faultHandlers>
		</mate:RemoteObjectInvoker>
	</mate:EventHandlers>
	
	<mate:Injectors target="{GridConsultaPedidos}">
		<mate:PropertyInjector source="{ModeloConsultaPedidosNuevo}"
							   sourceKey="encuentraTotalPorPeriodoPosPasado"
							   targetKey="recibeTotalPorPeriodo"/>
