	</mate:Injectors>	
	
	<mate:Injectors				target="{HistorialConsultasPedidoNuevo}">
		<mate:PropertyInjector source="{ModeloConsultaPedidosNuevo}" 
							   sourceKey="TiempoProcesosEtiqueta"
							   targetKey="recibeDatosEtiqueta"/>
	</mate:Injectors>	
	
