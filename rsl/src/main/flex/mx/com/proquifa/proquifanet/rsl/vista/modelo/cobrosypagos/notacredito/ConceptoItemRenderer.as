package mx.com.proquifa.proquifanet.rsl.vista.modelo.cobrosypagos.notacredito
{
	import mx.com.proquifa.proquifanet.rsl.vista.modelo.comun.Empresa;

	
	[Bindable]
	public class ConceptoItemRenderer
	{
		
		private var _finicio:Date;
		private var _ffin:Date;
		private var _tipo:String; //Periodo -- Factura
		private var _factura:Number;
		private var _percentNota:Number;
		private var _monto:Number;
		
		private var _empresaQueFactura:Empresa;
		private var _idCliente:Number;
		
		public function ConceptoItemRenderer()
		{
			finicio = new Date();
			ffin = new Date();
			tipo = 'Periodo';
			factura = percentNota = monto = 0;
		}

		public function get finicio():Date
		{
			return _finicio;
		}

		public function set finicio(value:Date):void
		{
			_finicio = value;
		}

		public function get ffin():Date
		{
			return _ffin;
		}

		public function set ffin(value:Date):void
		{
			_ffin = value;
		}

		public function get tipo():String
		{
			return _tipo;
		}

		public function set tipo(value:String):void
		{
			_tipo = value;
		}

		public function get factura():Number
		{
			return _factura;
		}

		public function set factura(value:Number):void
		{
			_factura = value;
		}

		public function get percentNota():Number
		{
			return _percentNota;
		}

		public function set percentNota(value:Number):void
		{
			_percentNota = value;
		}

		public function get monto():Number
		{
			return _monto;
		}

		public function set monto(value:Number):void
		{
			_monto = value;
		}

		public function get empresaQueFactura():Empresa
		{
			return _empresaQueFactura;
		}

		public function set empresaQueFactura(value:Empresa):void
		{
			_empresaQueFactura = value;
		}

		public function get idCliente():Number
		{
			return _idCliente;
		}

		public function set idCliente(value:Number):void
		{
			_idCliente = value;
		}


	}
}