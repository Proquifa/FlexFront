package mx.com.proquifa.proquifanet.rsl.vista.modelo.compras
{
	[RemoteClass(alias="mx.com.proquifa.proquifanet.modelo.compras.InformePCompra")]
	[Bindable]
	public class InformePCompra
	{
		
		private var _fechaInspeccion:Date;
		
		private var _etapaInforme:String;
		private var _inspector:String;
		private var _caducidad:String;
		private var _folioFD:String;
		
		private var _colocarInformePC:Boolean;
		
		public function InformePCompra()
		{
		}

		public function get fechaInspeccion():Date
		{
			return _fechaInspeccion;
		}

		public function set fechaInspeccion(value:Date):void
		{
			_fechaInspeccion = value;
		}

		public function get etapaInforme():String
		{
			return _etapaInforme;
		}

		public function set etapaInforme(value:String):void
		{
			_etapaInforme = value;
		}

		public function get inspector():String
		{
			return _inspector;
		}

		public function set inspector(value:String):void
		{
			_inspector = value;
		}

		public function get caducidad():String
		{
			return _caducidad;
		}

		public function set caducidad(value:String):void
		{
			_caducidad = value;
		}

		public function get folioFD():String
		{
			return _folioFD;
		}

		public function set folioFD(value:String):void
		{
			_folioFD = value;
		}

		public function get colocarInformePC():Boolean
		{
			return _colocarInformePC;
		}

		public function set colocarInformePC(value:Boolean):void
		{
			_colocarInformePC = value;
		}


	}
}