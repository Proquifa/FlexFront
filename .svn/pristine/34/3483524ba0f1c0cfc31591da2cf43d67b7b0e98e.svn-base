package mx.com.proquifa.proquifanet.rsl.vista.modelo.comun
{
	public class InformacionVentana
	{
		private var _idVentana:String;
		private var _informacion:Object;
		private var _idVentanaCerrar:String;
		
		private var _idCliente:Number;
		private var _nombreCliente:String;
		
		public function InformacionVentana()
		{
		}
		
		public function get nombreCliente():String
		{
			return _nombreCliente;
		}

		public function set nombreCliente(value:String):void
		{
			_nombreCliente = value;
		}

		public function get idCliente():Number
		{
			return _idCliente;
		}

		public function set idCliente(value:Number):void
		{
			_idCliente = value;
		}

		public function get idVentanaCerrar():String
		{
			return _idVentanaCerrar;
		}

		public function set idVentanaCerrar(value:String):void
		{
			_idVentanaCerrar = value;
		}

		public function get informacion():Object
		{
			return _informacion;
		}

		public function set informacion(value:Object):void
		{
			_informacion = value;
		}

		public function get idVentana():String
		{
			return _idVentana;
		}

		public function set idVentana(value:String):void
		{
			_idVentana = value;
		}

	}
}