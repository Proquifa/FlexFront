<?xml version="1.0" encoding="utf-8"?>
<s:VGroup xmlns:fx="http://ns.adobe.com/mxml/2009" 
		 xmlns:s="library://ns.adobe.com/flex/spark" 
		 xmlns:mx="library://ns.adobe.com/flex/mx" 
		 width="100%"
		 gap="0"
		 horizontalAlign="center"
		 >
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	<fx:Script>
		<![CDATA[
			[Bindable]	public var text:String="unset";
		]]>
	</fx:Script>
	<s:Rect width="100%" height="3">
		<s:fill>
			<s:SolidColor color="0x424242"/>
		</s:fill>
	</s:Rect>
	<s:Rect width="3" height="10">
		<s:fill>
			<s:SolidColor color="0x424242"/>
		</s:fill>
	</s:Rect>
	<s:Label fontFamily="Helvetica" fontWeight="bold" fontSize="10" fontStyle="normal" text="{text}" textAlign="center"
			 paddingTop="5" width="100%" paddingRight="-20" paddingLeft="-20"/>
</s:VGroup>
