				var evento:EventoComboCatalogos = new EventoComboCatalogos( EventoComboCatalogos.BUSQUEDA_PROCESOS );
				dispatchEvent( evento );
				this._datoExtra_BUSQUEDA_PROCESOS = valorAdicional;
