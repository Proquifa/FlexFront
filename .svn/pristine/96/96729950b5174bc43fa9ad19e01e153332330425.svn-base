			public function set recibirListaClientes(datos:ArrayCollection):void
			{
				if(datos == null)
					return;
				
				_datos = new ArrayCollection();
				_datos = datos;
				if( _primerItem != "" ){
					var firstItem:CatalogoItem = new CatalogoItem();
					firstItem.valor = _primerItem;
					firstItem.llave = 0;
					_datos.addItemAt( firstItem, 0 );
					_primerItem = "";
				}
				selectedIndex = 0;
			}
			
