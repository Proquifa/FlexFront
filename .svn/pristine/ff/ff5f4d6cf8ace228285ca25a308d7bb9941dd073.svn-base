<?xml version="1.0" encoding="utf-8"?>
<s:Group xmlns:fx="http://ns.adobe.com/mxml/2009" 
		 xmlns:s="library://ns.adobe.com/flex/spark" 
		 xmlns:mx="library://ns.adobe.com/flex/mx" width="100%" height="35">
	<fx:Script>
		<![CDATA[
			import skins.catalogos.catalogoColores;
			[Bindable]
			public var leftLabel:String;
			[Bindable]
			public var rightLabel:String;
			
		]]>
	</fx:Script>
	<s:Rect width="100%"  height="100%">
		<s:fill><s:SolidColor color="0x{catalogoColores.LABEL_CASI_NEGRO}" /></s:fill>
	</s:Rect>
	<s:HGroup width="100%" height="100%" verticalAlign="middle">
		<s:Label text="{leftLabel}" styleName="rpte_p4dlle_subheaders_left"/>
		<s:Spacer width="100%" height="5"/>
		<s:Label text="{rightLabel}" styleName="rpte_p4dlle_subheaders_right"/>
	</s:HGroup>
</s:Group>
