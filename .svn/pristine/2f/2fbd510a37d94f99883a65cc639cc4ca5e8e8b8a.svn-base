		<mate:MethodInvoker arguments="{[currentEvent.mensaje,currentEvent.tipoEventos,currentEvent.idButtons,currentEvent.error]}"
							generator="{ModeloTableroOperativos}"
							method="setControlDeBloqueoPantalla"/>
	</mate:EventHandlers>
	<mate:Injectors targets="{[TabOperativosBarraMenu]}">
		<mate:PropertyInjector source="{ModeloTableroOperativos}"
							   sourceKey="actualizarEstadoDeEsperaYBloqueo"
							   targetKey="actualizarEsperaYBloqueo"/>
	</mate:Injectors>
	
