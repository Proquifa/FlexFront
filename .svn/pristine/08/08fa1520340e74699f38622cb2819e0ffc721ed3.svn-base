						var soliFormulaClasif:EventoCatProOferta = new EventoCatProOferta(EventoCatProOferta.OBTENER_INFORMACION_DE_LA_FORMULA_PRECIO_CLASIFICACION);
						soliFormulaClasif.idProveedor = (_currentConfPrecioProducto as ConfiguracionPrecioProducto).idProveedor;
						soliFormulaClasif.idConfigClasificacion = (_currentConfPrecioProducto as ConfiguracionPrecioProducto).idConfiguracionClasificacion;
						soliFormulaClasif.idProducto = currentProducto.idProducto;
						
						
