						currentProducto.monedaSimbolo + _currentFormulaPrecio.permisoString + " + " + currentProducto.monedaSimbolo + _currentFormulaPrecio.fleteDestinoString + " = " +
						currentProducto.monedaSimbolo + costoDeVentaString;
				}
