			public function get dataProvider():ConfiguracionPrecioProducto
			{
				return _currentConfPrecioProducto;
			}
			
			public function set dataProvider(value:ConfiguracionPrecioProducto):void
			{
				if(value != null  )
				{
					if(value.idClienteConfiguracion == 0){
						lblNivelIngreso.text = "No hay configuración";			
					}else{
						_currentConfPrecioProducto = value;
						grpFactoresSinInfo.visible = grpFactoresSinInfo.includeInLayout = false;
						grpFactoresConInformacion.visible = true;
						currentCostoFactor = value.costoFactorProducto;
						
						armartextosParaBotonesNivelesIngreso(value); 
						mostrarInfoEnElPanel();
						
						lblPrecioLista.text=UtilsFormatosNumericos.redondearNEAREST(_currentConfPrecioProducto.costoProducto) +' '+ currentProducto.monedaAbreviatura;
						lblPrecioListaNeto.text=UtilsFormatosNumericos.precisionADosDecimalesConComas(_currentConfPrecioProducto.costoCompra) +' '+ currentProducto.monedaAbreviatura;
					}
				}
				else
				{
					//limpiar panel
					grpFactoresSinInfo.visible = grpFactoresSinInfo.includeInLayout = true;
					grpFactoresConInformacion.visible = false;
				}
			}
			
			
			public function mostrarInfoEnElPanel():void
			{
				limpiarSeleccionBtnNiveles();
				/*btnAAplus.selected = true;*/ 
				btnAAplus.selected = true; 
				_currentNI = "AA+";
				
