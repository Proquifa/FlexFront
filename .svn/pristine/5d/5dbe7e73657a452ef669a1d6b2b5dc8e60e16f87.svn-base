<?xml version="1.0" encoding="utf-8"?>
<s:Scroller xmlns:fx="http://ns.adobe.com/mxml/2009" 
			xmlns:s="library://ns.adobe.com/flex/spark" 
			xmlns:mx="library://ns.adobe.com/flex/mx" 
			width="100%" height="100%">
	<fx:Script>
		<![CDATA[
			import mx.collections.ArrayCollection;
			
			[Bindable] public var _cobro:ArrayCollection = new ArrayCollection();
		]]>
	</fx:Script>
	<s:VGroup width="100%" height="100%" >
		<s:DataGroup itemRenderer="mx.com.proquifa.proquifanet.rsl.vista.vistas.consultas.finanzas.facturacion.itemRenderer.IRConsulta3Cobro_Facturacion" 
					 width="100%" dataProvider="{_cobro}">
			<s:layout>
				<s:VerticalLayout useVirtualLayout="false" requestedMinRowCount="1"/>
			</s:layout>
		</s:DataGroup>
	</s:VGroup>  
</s:Scroller>
