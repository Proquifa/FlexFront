<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:s="library://ns.adobe.com/flex/spark"
		xmlns:fx="http://ns.adobe.com/mxml/2009"
		minWidth="72"
		height="20"
		>
	<fx:Metadata>[HostComponent("spark.components.Button")]</fx:Metadata>
	<s:states>
		<s:State name="up"/>
		<s:State name="over"/>
		<s:State name="down"/>
		<s:State name="disabled"/>
	</s:states>
	<s:Rect id="rectFondo" left="-25" right="0" top="-10" bottom="0" >
		<s:fill>
				<s:SolidColor id="fondoFill" alpha="0" alpha.down="0.18" />
		</s:fill>
		<s:fill.disabled>
			<s:SolidColor id="fondoDisable" alpha="0" />
		</s:fill.disabled>
		<!--<s:stroke.disabled>
			<s:SolidColorStroke id="fondoStrokeDisable" caps="none" joints="miter" miterLimit="4" weight="1" alpha="0.1" />
		</s:stroke.disabled>-->
	</s:Rect>
	
	<s:RichText id="labelDisplay" width="100%" textAlign="right" top="0" maxDisplayedLines="1" 
				color="#FFFFFF"
				alpha.over="0.2" alpha.disabled="0.9" 
				fontFamily="Novecento" fontWeight="bold" fontStyle="normal" fontSize="14" tabStops="S0 S50 S100" />
	<s:Rect left="-25" right="0" height="1" bottom="0" >
			<s:fill>
				<s:SolidColor id="lineaUnderline" alpha="1" alpha.disabled="0.9" />
			</s:fill>
	</s:Rect>
</s:Skin>