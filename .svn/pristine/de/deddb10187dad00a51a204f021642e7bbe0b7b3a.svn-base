package modelo.objetos
{
	[Bindable]
	[RemoteClass (alias="mx.com.proquifa.proquifanet.modelo.comun.Documento")]
	public class Documento
	{
		
		private var _folio:String;
		private var _tipo:String;
		private var _evento:String;
		private var _destino:String;
		private var _verificado:Boolean;

		
		public function Documento()
		{
		}

		public function get verificado():Boolean
		{
			return _verificado;
		}

		public function set verificado(value:Boolean):void
		{
			_verificado = value;
		}

		public function get destino():String
		{
			return _destino;
		}

		public function set destino(value:String):void
		{
			_destino = value;
		}

		public function get evento():String
		{
			return _evento;
		}

		public function set evento(value:String):void
		{
			_evento = value;
		}

		public function get tipo():String
		{
			return _tipo;
		}

		public function set tipo(value:String):void
		{
			_tipo = value;
		}

		public function get folio():String
		{
			return _folio;
		}

		public function set folio(value:String):void
		{
			_folio = value;
		}
		
		
		
	}
}