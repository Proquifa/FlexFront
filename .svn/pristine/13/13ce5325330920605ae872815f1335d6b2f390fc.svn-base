<?xml version="1.0" encoding="utf-8"?>
<mx:PieSeries xmlns:fx="http://ns.adobe.com/mxml/2009" 
			  xmlns:s="library://ns.adobe.com/flex/spark" 
			  xmlns:mx="library://ns.adobe.com/flex/mx">
	<fx:Script>
		<![CDATA[
			import mx.effects.easing.Elastic;
		]]>
	</fx:Script>
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	<mx:fills>
		<s:LinearGradient rotation="90">
			<s:GradientEntry alpha="1.0" color="#f9af48" ratio="0"/>
			<s:GradientEntry alpha="1.0" color="#f9af48" ratio="1"/>
		</s:LinearGradient>
		<s:LinearGradient rotation="90">
			<s:GradientEntry alpha="1.0" color="#8dc449" ratio="0"/>
			<s:GradientEntry alpha="1.0" color="#8dc449" ratio="1"/>
		</s:LinearGradient>
		<s:LinearGradient rotation="90">
			<s:GradientEntry alpha="1.0" color="#139148" ratio="0"/>
			<s:GradientEntry alpha="1.0" color="#139148" ratio="1"/>
		</s:LinearGradient>
		<s:LinearGradient rotation="90">
			<s:GradientEntry alpha="1.0" color="#1073b9" ratio="0"/>
			<s:GradientEntry alpha="1.0" color="#1073b9" ratio="1"/>
		</s:LinearGradient>
		<s:LinearGradient rotation="90">
			<s:GradientEntry alpha="1.0" color="#912d8d" ratio="0"/>
			<s:GradientEntry alpha="1.0" color="#912d8d" ratio="1"/>
		</s:LinearGradient>
		<s:LinearGradient rotation="90">
			<s:GradientEntry alpha="1.0" color="#d21b5c" ratio="0"/>
			<s:GradientEntry alpha="1.0" color="#d21b5c" ratio="1"/>
		</s:LinearGradient>
		<s:LinearGradient rotation="90">
			<s:GradientEntry alpha="1.0" color="#ea267a" ratio="0"/>
			<s:GradientEntry alpha="1.0" color="#ea267a" ratio="1"/>
		</s:LinearGradient>
	</mx:fills>
	<mx:filters>
		<s:DropShadowFilter distance="1" quality="3"
					blurX="2" blurY="2"
					color="0x000000" alpha=".8"/>
	</mx:filters>
	<mx:showDataEffect>
		<mx:SeriesInterpolate duration="1500" easingFunction="{Elastic.easeOut}"/>
	</mx:showDataEffect>
</mx:PieSeries>
