package mx.com.proquifa.proquifanet.rsl.vista.modelo.catalogos.proveedores
{
	import mx.collections.ArrayCollection;

	[RemoteClass(alias="mx.com.proquifa.proquifanet.modelo.catalogos.proveedores.InformacionPagos")]
	[Bindable]
	public class InformacionPagos
	{
		
		private var _idProveedor:Number;
		private var _condicionesPago:String;
		private var _lineaCredito:Number;
		private var _limiteCredito:Number;
		private var _comentarios:String;
		private var _medios:ArrayCollection;
		private var _fua:Date;

		public function InformacionPagos()
		{
		}

		public function get idProveedor():Number
		{
			return _idProveedor;
		}

		public function set idProveedor(value:Number):void
		{
			_idProveedor = value;
		}

		public function get condicionesPago():String
		{
			return _condicionesPago;
		}

		public function set condicionesPago(value:String):void
		{
			_condicionesPago = value;
		}

		public function get lineaCredito():Number
		{
			return _lineaCredito;
		}

		public function set lineaCredito(value:Number):void
		{
			_lineaCredito = value;
		}

		public function get limiteCredito():Number
		{
			return _limiteCredito;
		}

		public function set limiteCredito(value:Number):void
		{
			_limiteCredito = value;
		}

		public function get comentarios():String
		{
			return _comentarios;
		}

		public function set comentarios(value:String):void
		{
			_comentarios = value;
		}

		public function get medios():ArrayCollection
		{
			return _medios;
		}

		public function set medios(value:ArrayCollection):void
		{
			_medios = value;
		}

		public function get fua():Date
		{
			return _fua;
		}

		public function set fua(value:Date):void
		{
			_fua = value;
		}
	}
}