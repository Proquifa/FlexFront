package mx.com.proquifa.proquifanet.rsl.vista.modelo.ventas.visita
{
	import mx.collections.ArrayCollection;

	[Bindable]
	[RemoteClass(alias="mx.com.proquifa.proquifanet.modelo.comun.HallazgosAcciones")]
	public class HallazgosAcciones
	{
		
		private var _tipo:String;
		private var _descripcion:String;
		private var _idVisita:int;
		
		private var _ferealizacion:Date;
		private var _frealizacion:Date;
		private var _observaciones:String;
		private var _tipoHallazgo:String;
		private var _competencia:String;
		private var _marcas:ArrayCollection;
		private var _idHallazgoAccion:Number;
		private var _documento:DocumentoSolicitudVisita;
		
		public function HallazgosAcciones()
		{
			documento = new DocumentoSolicitudVisita;
			documento.nombre = ".pdf";
		}

		public function get idVisita():int
		{
			return _idVisita;
		}

		public function set idVisita(value:int):void
		{
			_idVisita = value;
		}

		public function get descripcion():String
		{
			return _descripcion;
		}

		public function set descripcion(value:String):void
		{
			_descripcion = value;
		}

		public function get tipo():String
		{
			return _tipo;
		}

		public function set tipo(value:String):void
		{
			_tipo = value;
		}

		public function get ferealizacion():Date
		{
			return _ferealizacion;
		}

		public function set ferealizacion(value:Date):void
		{
			_ferealizacion = value;
		}

		public function get frealizacion():Date
		{
			return _frealizacion;
		}

		public function set frealizacion(value:Date):void
		{
			_frealizacion = value;
		}

		public function get observaciones():String
		{
			return _observaciones;
		}

		public function set observaciones(value:String):void
		{
			_observaciones = value;
		}

		public function get tipoHallazgo():String
		{
			return _tipoHallazgo;
		}

		public function set tipoHallazgo(value:String):void
		{
			_tipoHallazgo = value;
		}

		public function get competencia():String
		{
			return _competencia;
		}

		public function set competencia(value:String):void
		{
			_competencia = value;
		}

		public function get marcas():ArrayCollection
		{
			return _marcas;
		}

		public function set marcas(value:ArrayCollection):void
		{
			_marcas = value;
		}

		public function get idHallazgoAccion():Number
		{
			return _idHallazgoAccion;
		}

		public function set idHallazgoAccion(value:Number):void
		{
			_idHallazgoAccion = value;
		}

		public function get documento():DocumentoSolicitudVisita
		{
			return _documento;
		}

		public function set documento(value:DocumentoSolicitudVisita):void
		{
			_documento = value;
		}


	}
}