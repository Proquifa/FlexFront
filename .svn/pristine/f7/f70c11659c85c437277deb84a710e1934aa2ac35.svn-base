						<linkButton:LinkButtonPQNet2 label="{_informacion.numCheque.slice(-4,_informacion.numCheque.length)}" click="mostrarChequeFirmado()" id="lnkNumChequeCheque" />
						<label:Texto_w100 id="lblNumChequeCheque" text="{valor}"/>
						<label:Titulo_w100  text="Monto a pagar:"/>
						<s:Label text="$ {UtilsFormatosNumericos.precisionADosDecimalesConComas(_informacion.importe)} USD" />
						<mx:VBox visible="false" id="vgpChequeFirmado" width="100%">
							<label:Titulo_w100  text="¿Cheque firmado?"/>
							<label:Texto_w100  text="{_informacion.chequeFirmado}"/>
						</mx:VBox>
					</s:VGroup>
				</s:VGroup>  
				
				<s:VGroup id="grpTituloInconsistencias" width="100%" paddingBottom="10">
					<panelCtaDetalle:SubHeader2  label="INCONSISTENCIAS"/>
					<s:VGroup width="100%" id="vgpInconsistencias" paddingLeft="15" />
				</s:VGroup>
				
				<s:VGroup id="grpTituloInconsistenciasRevisar" width="100%" paddingBottom="10">
					<panelCtaDetalle:SubHeader2 label="INCONSISTENCIAS: REVISAR FACTURA"/>
					<s:VGroup width="100%" id="vgpInconsistenciasRevisar" visible="true" paddingLeft="15"  />
				</s:VGroup>  
				
				<s:VGroup id="grpTituloInconsistenciasControlar" width="100%" paddingBottom="10">
					<panelCtaDetalle:SubHeader2 label="INCONSISTENCIAS: CONTROLAR PAGO REALIZADO"/>
					<s:VGroup width="100%" id="vgpInconsistenciasControlar" visible="true" paddingLeft="15" />
				</s:VGroup>  
				
			</s:VGroup>
		</s:VGroup>
	</s:Scroller>
</s:VGroup>
