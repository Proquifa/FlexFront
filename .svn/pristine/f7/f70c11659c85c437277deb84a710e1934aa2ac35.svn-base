			public function crearInconsistencias( $resumen:ArrayCollection ):void{
				
				vgpInconsistencias.removeAllElements();
				var num:int = 1;
				for (var j:int = 1; j <$resumen.length; j++){
					if ($resumen[j].inconsistencia != null && $resumen[j].inconsistencia != ""){
						
						var lblNum:TituloGris = new TituloGris;
						lblNum.text = "#" + num;
						
						var lblInconsis:TituloGris = new TituloGris;
						lblInconsis.text = "Inconsistencia:";
						
						var lblTexto:TextoGris_w100 = new TextoGris_w100;
						$resumen[j].inconsistencia == null ? lblTexto.text = valor : lblTexto.text = $resumen[j].inconsistencia;
						
						var lblAclara:TituloGris = new TituloGris;
						lblAclara.text = "Aclaración:";
						
						var lblTextoAclara:TextoGris_w100 = new TextoGris_w100;
						$resumen[j].aclaracion == null ? lblTextoAclara.text = valor : lblTextoAclara.text = $resumen[j].aclaracion;
						
						var div:division = new division();
						div.percentWidth = 100;
						
						vgpInconsistencias.addElement(lblNum);
						vgpInconsistencias.addElement(lblInconsis);
						vgpInconsistencias.addElement(lblTexto);
						vgpInconsistencias.addElement(lblAclara);
						vgpInconsistencias.addElement(lblTextoAclara);
						vgpInconsistencias.addElement(div);
						num++;
					}
				}
				if (num != 1){
					grpTituloInconsistencias.visible = grpTituloInconsistencias.includeInLayout = true;
				}
				
			}
			
			public function crearInconsistenciasPago( $resumen:ArrayCollection ):void{
				var numControlar:int = 1;
				var numRevisar:int = 1;
				vgpInconsistenciasRevisar.removeAllElements();
				vgpInconsistenciasControlar.removeAllElements();
				for (var j:int = 0; j < $resumen.length; j++){
					var num:int = 0;
					var lblNum:TituloGris = new TituloGris;
					var lblInconsis:TituloGris= new TituloGris;
					var lblTexto:TextoGris_w100 = new TextoGris_w100;
					var lblAclara:TituloGris = new TituloGris;
					var lblTextoAclara:TextoGris_w100 = new TextoGris_w100;
					var div:division = new division();
					if ($resumen[j].inconsistencias != null && $resumen[j].inconsistencias != ""){
						if ( $resumen[j].tipo == "ControlarPagoRealizado" ){
							num = numControlar;
							vgpInconsistenciasControlar.addElement(lblNum);
							vgpInconsistenciasControlar.addElement(lblInconsis);
							vgpInconsistenciasControlar.addElement(lblTexto);
							vgpInconsistenciasControlar.addElement(lblAclara);
							vgpInconsistenciasControlar.addElement(lblTextoAclara);
							vgpInconsistenciasControlar.addElement(div);
							numControlar++;
						}else{
							num = numRevisar;
							vgpInconsistenciasRevisar.addElement(lblNum);
							vgpInconsistenciasRevisar.addElement(lblInconsis);
							vgpInconsistenciasRevisar.addElement(lblTexto);
							vgpInconsistenciasRevisar.addElement(lblAclara);
							vgpInconsistenciasRevisar.addElement(lblTextoAclara);
							vgpInconsistenciasRevisar.addElement(div);
							numRevisar++;
						}
						
						lblNum.text = "#" + num;
						lblInconsis.text = "Inconsistencia:";
						lblTexto.text = $resumen[j].inconsistencias;
						lblAclara.text = "Aclaración:";
						lblTextoAclara.text = $resumen[j].aclaracion;
						div.percentWidth = 100;
					}
				}
				if (numControlar != 1){
					grpTituloInconsistenciasControlar.visible = grpTituloInconsistenciasControlar.includeInLayout = true;
				}
				if (numRevisar != 1){
					grpTituloInconsistenciasRevisar.visible = grpTituloInconsistenciasRevisar.includeInLayout = true;
				}
				
			}
			
			public function set recibeParametrosHistorial( $evt:EventoConsultaPagos ):void{
				if ($evt == null ){
					return;
				}
				
				_parametros = $evt;
				if (_parametros.estado != null && _parametros.estado != "" ){
					grpTituloPrincipal.tipoDeBandera = "verde";
					valor = "ND";
				}else{
					grpTituloPrincipal.tipoDeBandera = "roja";
					valor = "Pendiente";
				}
			}
			
			public function set recibeInformacionGeneral( $info:ArrayCollection ):void{
