			}
			
			public function mostrarDeposito( doc:String):void{				
				mostrarDocumento( RutasPdF.GENERAL_RUTA + RutasPdF.DEPOSITOS_RUTA + doc + ".pdf" );
			}
			
			public function mostrarOtros():void{
				if ( _informacion.documentos != null ){
					var docs:Array = _informacion.documentos.split(",");
					for (var i:int = 0; i < docs.length; i++){
						mostrarDocumento( RutasPdF.GENERAL_RUTA + RutasPdF.FACTURAS_PROVEEDOR_OTROS+ docs[i] + ".pdf" );
					}
				}
			}
			
			public function mostrarCorreoConfirmacion():void{
				mostrarDocumento( RutasPdF.GENERAL_RUTA + RutasPdF.PAGOS_CONFIRMACION_RUTA + _informacion.correoC + ".pdf" );
			}
			
			public function mostrarFolioPagos():void{
				mostrarDocumento( RutasPdF.GENERAL_RUTA + RutasPdF.PAGOS_RUTA +  _informacion.folioPG + ".pdf" );
			}
			
