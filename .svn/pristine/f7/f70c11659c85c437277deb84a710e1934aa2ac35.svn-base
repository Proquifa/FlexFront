						lnkFacturaCargar.visible = lnkFacturaCargar.includeInLayout = true;
						
						if ($info[i].documentos == null ){
							lnkOtrosCargar.visible = lnkOtrosCargar.includeInLayout = false;
							lblOtrosCargar.visible = lblOtrosCargar.includeInLayout = true;
						}else{
							lnkOtrosCargar.visible = lnkOtrosCargar.includeInLayout = true;
