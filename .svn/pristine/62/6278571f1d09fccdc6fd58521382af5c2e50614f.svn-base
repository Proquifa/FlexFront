package mx.com.proquifa.vista.modelo.RegistrarArribp
{
	import mx.collections.ArrayCollection;

	[Bindable]
	public class ProductosArribados
	{
		private var _numFila:int;
		private var _nombreFletera:String;
		private var _descripcion:String;
		private var _pzsXarribar:int;
		private var _pzsArribadas:int;
		private var _pzsFaltantes:int;
		private var _pzsSobrantes:int;
		private var _listaProductos:ArrayCollection;
		
		
		public function ProductosArribados()
		{
			_numFila = 0;
			_nombreFletera = "";
			_descripcion = "";
			_pzsXarribar = 0;
			_pzsArribadas = -1;
			_pzsFaltantes = 0;
			_pzsSobrantes = 0;
			_listaProductos = new ArrayCollection();
		}
		

		public function get numFila():int
		{
			return _numFila;
		}

		public function set numFila(value:int):void
		{
			_numFila = value;
		}

		public function get nombreFletera():String
		{
			return _nombreFletera;
		}

		public function set nombreFletera(value:String):void
		{
			_nombreFletera = value;
		}

		public function get descripcion():String
		{
			return _descripcion;
		}

		public function set descripcion(value:String):void
		{
			_descripcion = value;
		}

		public function get pzsXarribar():int
		{
			return _pzsXarribar;
		}

		public function set pzsXarribar(value:int):void
		{
			_pzsXarribar = value;
		}

		public function get pzsArribadas():int
		{
			return _pzsArribadas;
		}

		public function set pzsArribadas(value:int):void
		{
			_pzsArribadas = value;
		}

		public function get pzsFaltantes():int
		{
			return _pzsFaltantes;
		}

		public function set pzsFaltantes(value:int):void
		{
			_pzsFaltantes = value;
		}

		public function get pzsSobrantes():int
		{
			return _pzsSobrantes;
		}

		public function set pzsSobrantes(value:int):void
		{
			_pzsSobrantes = value;
		}

		public function get listaProductos():ArrayCollection
		{
			return _listaProductos;
		}

		public function set listaProductos(value:ArrayCollection):void
		{
			_listaProductos = value;
		}


	}
}