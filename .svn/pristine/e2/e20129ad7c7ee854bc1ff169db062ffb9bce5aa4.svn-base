package mx.com.proquifa.proquifanet.rsl.vista.modelo.comun
{
	[RemoteClass(alias="mx.com.proquifa.proquifanet.modelo.comun.Modulo")]
	public class Modulo
	{
		private var _idModulo:Number;
		
		private var _nombre:String;
		private var _nombre_vista:String;
		private var _tipo:String;
		//variables agregadas de prueba
		private var _funcion:String;
		private var _direccion:String = "http://www.proquifa.com.mx:8080/proq.gif";
		private var _iconMobil:String;
		
		public function Modulo()
		{
		}

		public function get iconMobil():String
		{
			return _iconMobil;
		}

		public function set iconMobil(value:String):void
		{
			_iconMobil = value;
		}

		public function get direccion():String
		{
			return _direccion;
		}

		public function set direccion(value:String):void
		{
			_direccion = value;
		}

		public function get funcion():String
		{
			return _funcion;
		}

		public function set funcion(value:String):void
		{
			_funcion = value;
		}

		public function get tipo():String
		{
			return _tipo;
		}

		public function set tipo(value:String):void
		{
			_tipo = value;
		}

		public function get nombre_vista():String
		{
			return _nombre_vista;
		}

		public function set nombre_vista(value:String):void
		{
			_nombre_vista = value;
		}

		public function get nombre():String
		{
			return _nombre;
		}

		public function set nombre(value:String):void
		{
			_nombre = value;
		}

		public function get idModulo():Number
		{
			return _idModulo;
		}

		public function set idModulo(value:Number):void
		{
			_idModulo = value;
		}

	}
}