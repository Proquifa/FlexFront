<?xml version="1.0" encoding="utf-8"?>
<mx:PieSeries xmlns:fx="http://ns.adobe.com/mxml/2009" 
			  xmlns:s="library://ns.adobe.com/flex/spark" 
			  xmlns:mx="library://ns.adobe.com/flex/mx">
	<fx:Script>
		<![CDATA[
			import mx.effects.easing.Elastic;
		]]>
	</fx:Script>
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	<mx:fills>
		<s:LinearGradient rotation="90">
			<s:GradientEntry alpha="1.0" color="#f5ab54" ratio="0"/>
			<s:GradientEntry alpha="1.0" color="#f5ab54" ratio="1"/>
		</s:LinearGradient>
		<s:LinearGradient rotation="90">
			<s:GradientEntry alpha="1.0" color="#f5aa60" ratio="0"/>
			<s:GradientEntry alpha="1.0" color="#f5aa60" ratio="1"/>
		</s:LinearGradient>
		<s:LinearGradient rotation="90">
			<s:GradientEntry alpha="1.0" color="#f5ae6f" ratio="0"/>
			<s:GradientEntry alpha="1.0" color="#f5ae6f" ratio="1"/>
		</s:LinearGradient>
		<s:LinearGradient rotation="90">
			<s:GradientEntry alpha="1.0" color="#f6b17c" ratio="0"/>
			<s:GradientEntry alpha="1.0" color="#f6b17c" ratio="1"/>
		</s:LinearGradient>
		<s:LinearGradient rotation="90">
			<s:GradientEntry alpha="1.0" color="#f7b88b" ratio="0"/>
			<s:GradientEntry alpha="1.0" color="#f7b88b" ratio="1"/>
		</s:LinearGradient>
		<s:LinearGradient rotation="90">
			<s:GradientEntry alpha="1.0" color="#f3ba95" ratio="0"/>
			<s:GradientEntry alpha="1.0" color="#f3ba95" ratio="1"/>
		</s:LinearGradient>
		<s:LinearGradient rotation="90">
			<s:GradientEntry alpha="1.0" color="#f8c4a2" ratio="0"/>
			<s:GradientEntry alpha="1.0" color="#f8c4a2" ratio="1"/>
		</s:LinearGradient>
		<s:LinearGradient rotation="90">
			<s:GradientEntry alpha="1.0" color="#f8cdb1" ratio="0"/>
			<s:GradientEntry alpha="1.0" color="#f8cdb1" ratio="1"/>
		</s:LinearGradient>
		<s:LinearGradient rotation="90">
			<s:GradientEntry alpha="1.0" color="#f8d4bd" ratio="0"/>
			<s:GradientEntry alpha="1.0" color="#f8d4bd" ratio="1"/>
		</s:LinearGradient>
		<s:LinearGradient rotation="90">
			<s:GradientEntry alpha="1.0" color="#f6dbca" ratio="0"/>
			<s:GradientEntry alpha="1.0" color="#f6dbca" ratio="1"/>
		</s:LinearGradient>
		<s:LinearGradient rotation="90">
			<s:GradientEntry alpha="1.0" color="#f7e3d8" ratio="0"/>
			<s:GradientEntry alpha="1.0" color="#f7e3d8" ratio="1"/>
		</s:LinearGradient>
		<s:LinearGradient rotation="90">
			<s:GradientEntry alpha="1.0" color="#f4e8e1" ratio="0"/>
			<s:GradientEntry alpha="1.0" color="#f4e8e1" ratio="1"/>
		</s:LinearGradient>
	</mx:fills>
	<mx:filters>
		<s:DropShadowFilter distance="1" quality="3"
					blurX="2" blurY="2"
					color="0x000000" alpha=".8"/>
	</mx:filters>
	<mx:showDataEffect>
		<mx:SeriesInterpolate duration="1500" easingFunction="{Elastic.easeOut}"/>
	</mx:showDataEffect>
</mx:PieSeries>
