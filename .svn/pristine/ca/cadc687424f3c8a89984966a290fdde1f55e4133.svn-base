<?xml version="1.0" encoding="utf-8"?>
<mx:PieSeries xmlns:fx="http://ns.adobe.com/mxml/2009" 
			  xmlns:s="library://ns.adobe.com/flex/spark" 
			  xmlns:mx="library://ns.adobe.com/flex/mx">
	<fx:Script>
		<![CDATA[
			import mx.effects.easing.Elastic;
		]]>
	</fx:Script>
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	<mx:fills>
		<s:LinearGradient rotation="90">
			<s:GradientEntry alpha="1.0" color="#37b02f" ratio="0"/>
			<s:GradientEntry alpha="1.0" color="#37b02f" ratio="1"/>
		</s:LinearGradient>
		<s:LinearGradient rotation="90">
			<s:GradientEntry alpha="1.0" color="#44b537" ratio="0"/>
			<s:GradientEntry alpha="1.0" color="#44b537" ratio="1"/>
		</s:LinearGradient>
		<s:LinearGradient rotation="90">
			<s:GradientEntry alpha="1.0" color="#51b841" ratio="0"/>
			<s:GradientEntry alpha="1.0" color="#51b841" ratio="1"/>
		</s:LinearGradient>
		<s:LinearGradient rotation="90">
			<s:GradientEntry alpha="1.0" color="#5cba4b" ratio="0"/>
			<s:GradientEntry alpha="1.0" color="#5cba4b" ratio="1"/>
		</s:LinearGradient>
		<s:LinearGradient rotation="90">
			<s:GradientEntry alpha="1.0" color="#6bbf58" ratio="0"/>
			<s:GradientEntry alpha="1.0" color="#6bbf58" ratio="1"/>
		</s:LinearGradient>
		<s:LinearGradient rotation="90">
			<s:GradientEntry alpha="1.0" color="#76c361" ratio="0"/>
			<s:GradientEntry alpha="1.0" color="#76c361" ratio="1"/>
		</s:LinearGradient>
		<s:LinearGradient rotation="90">
			<s:GradientEntry alpha="1.0" color="#82c76c" ratio="0"/>
			<s:GradientEntry alpha="1.0" color="#82c76c" ratio="1"/>
		</s:LinearGradient>
		<s:LinearGradient rotation="90">
			<s:GradientEntry alpha="1.0" color="#8ccb75" ratio="0"/>
			<s:GradientEntry alpha="1.0" color="#8ccb75" ratio="1"/>
		</s:LinearGradient>
		<s:LinearGradient rotation="90">
			<s:GradientEntry alpha="1.0" color="#99d283" ratio="0"/>
			<s:GradientEntry alpha="1.0" color="#99d283" ratio="1"/>
		</s:LinearGradient>
		<s:LinearGradient rotation="90">
			<s:GradientEntry alpha="1.0" color="#a4d78e" ratio="0"/>
			<s:GradientEntry alpha="1.0" color="#a4d78e" ratio="1"/>
		</s:LinearGradient>
		<s:LinearGradient rotation="90">
			<s:GradientEntry alpha="1.0" color="#b1dc9c" ratio="0"/>
			<s:GradientEntry alpha="1.0" color="#b1dc9c" ratio="1"/>
		</s:LinearGradient>
		<s:LinearGradient rotation="90">
			<s:GradientEntry alpha="1.0" color="#c0e4ac" ratio="0"/>
			<s:GradientEntry alpha="1.0" color="#c0e4ac" ratio="1"/>
		</s:LinearGradient>
	</mx:fills>
	<mx:filters>
		<s:DropShadowFilter distance="1" quality="3"
					blurX="2" blurY="2"
					color="0x000000" alpha=".8"/>
	</mx:filters>
	<mx:showDataEffect>
		<mx:SeriesInterpolate duration="1500" easingFunction="{Elastic.easeOut}"/>
	</mx:showDataEffect>
</mx:PieSeries>
