<?xml version="1.0" encoding="utf-8"?>
<mx:PieSeries xmlns:fx="http://ns.adobe.com/mxml/2009" 
			  xmlns:s="library://ns.adobe.com/flex/spark" 
			  xmlns:mx="library://ns.adobe.com/flex/mx">
	<fx:Script>
		<![CDATA[
			import mx.effects.easing.Elastic;
		]]>
	</fx:Script>
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	<mx:fills>
		<s:LinearGradient rotation="90">
			<s:GradientEntry alpha="1.0" color="#c2333f" ratio="0"/>
			<s:GradientEntry alpha="1.0" color="#c2333f" ratio="1"/>
		</s:LinearGradient>
		<s:LinearGradient rotation="90">
			<s:GradientEntry alpha="1.0" color="#c43e4f" ratio="0"/>
			<s:GradientEntry alpha="1.0" color="#c43e4f" ratio="1"/>
		</s:LinearGradient>
		<s:LinearGradient rotation="90">
			<s:GradientEntry alpha="1.0" color="#c74a60" ratio="0"/>
			<s:GradientEntry alpha="1.0" color="#c74a60" ratio="1"/>
		</s:LinearGradient>
		<s:LinearGradient rotation="90">
			<s:GradientEntry alpha="1.0" color="#ca556c" ratio="0"/>
			<s:GradientEntry alpha="1.0" color="#ca556c" ratio="1"/>
		</s:LinearGradient>
		<s:LinearGradient rotation="90">
			<s:GradientEntry alpha="1.0" color="#cf627d" ratio="0"/>
			<s:GradientEntry alpha="1.0" color="#cf627d" ratio="1"/>
		</s:LinearGradient>
		<s:LinearGradient rotation="90">
			<s:GradientEntry alpha="1.0" color="#d16d89" ratio="0"/>
			<s:GradientEntry alpha="1.0" color="#d16d89" ratio="1"/>
		</s:LinearGradient>
		<s:LinearGradient rotation="90">
			<s:GradientEntry alpha="1.0" color="#d47995" ratio="0"/>
			<s:GradientEntry alpha="1.0" color="#d47995" ratio="1"/>
		</s:LinearGradient>
		<s:LinearGradient rotation="90">
			<s:GradientEntry alpha="1.0" color="#d685a3" ratio="0"/>
			<s:GradientEntry alpha="1.0" color="#d685a3" ratio="1"/>
		</s:LinearGradient>
		<s:LinearGradient rotation="90">
			<s:GradientEntry alpha="1.0" color="#dc93b1" ratio="0"/>
			<s:GradientEntry alpha="1.0" color="#dc93b1" ratio="1"/>
		</s:LinearGradient>
		<s:LinearGradient rotation="90">
			<s:GradientEntry alpha="1.0" color="#e1a2bf" ratio="0"/>
			<s:GradientEntry alpha="1.0" color="#e1a2bf" ratio="1"/>
		</s:LinearGradient>
		<s:LinearGradient rotation="90">
			<s:GradientEntry alpha="1.0" color="#e7b2cd" ratio="0"/>
			<s:GradientEntry alpha="1.0" color="#e7b2cd" ratio="1"/>
		</s:LinearGradient>
		<s:LinearGradient rotation="90">
			<s:GradientEntry alpha="1.0" color="#e9bfd6" ratio="0"/>
			<s:GradientEntry alpha="1.0" color="#e9bfd6" ratio="1"/>
		</s:LinearGradient>
	</mx:fills>
	<mx:filters>
		<s:DropShadowFilter distance="1" quality="3"
					blurX="2" blurY="2"
					color="0x000000" alpha=".8"/>
	</mx:filters>
	<mx:showDataEffect>
		<mx:SeriesInterpolate duration="1500" easingFunction="{Elastic.easeOut}"/>
	</mx:showDataEffect>
</mx:PieSeries>
