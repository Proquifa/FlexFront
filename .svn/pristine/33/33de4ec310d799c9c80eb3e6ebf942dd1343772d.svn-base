<?xml version="1.0" encoding="utf-8"?>
<s:Group xmlns:fx="http://ns.adobe.com/mxml/2009" 
		 xmlns:s="library://ns.adobe.com/flex/spark" 
		 xmlns:mx="library://ns.adobe.com/flex/mx" 
		 width="100%" height="100%"
		 xmlns:otros="skins.otros.*"
		 >
	
	<fx:Script>
		<![CDATA[
			import mx.com.proquifa.proquifanet.rsl.vista.eventos.comun.EventoComun;
			protected function regresarACatalogo(event:MouseEvent):void
			{
				dispatchEvent(new EventoComun(EventoComun.REGRESAR_A_MENU_CONFIGURACION));    
			}
		]]>
	</fx:Script>
	
	
	<s:Group width="100" height="60" click="regresarACatalogo(event)" >
		<s:Rect width="100" height="60">
			<s:fill>
				<s:SolidColor color="#cafe00" />
			</s:fill>
		</s:Rect>
	</s:Group>  
	
	<s:HGroup width="100%" height="100%" gap="100" >
		<otros:VDivision height="100%" width="1" color="0xFF0000" />
		<otros:VDivision height="100%" width="1" color="0x00FF00" />
	</s:HGroup>  
	
	
	
	
</s:Group>
