<?xml version="1.0" encoding="utf-8"?>
<s:HGroup xmlns:fx="http://ns.adobe.com/mxml/2009" 
		  xmlns:s="library://ns.adobe.com/flex/spark" 
		  xmlns:mx="library://ns.adobe.com/flex/mx" 
		  maxHeight="50" horizontalAlign="center" verticalAlign="middle" gap="60"
		  width="100%" height="100%"
		  >
	
	
	<fx:Script>
		<![CDATA[
			import skins.catalogos.catalogoIconosBotoneras;
		]]>
	</fx:Script>
	
	<s:HGroup height="100%" gap="10" verticalAlign="middle">
		<mx:Image smoothBitmapContent="true" source="{catalogoIconosBotoneras.getInstance().todos_20}" />
		<s:Label styleName="Texto_14_Helvetica" text="Todos" />
	</s:HGroup>  
	<s:HGroup height="100%" gap="10" verticalAlign="middle">
		<mx:Image smoothBitmapContent="true" source="{catalogoIconosBotoneras.getInstance().regular_mini}" />
		<s:Label styleName="Texto_14_Helvetica" text="Regular" />
	</s:HGroup>  
	<s:HGroup height="100%" gap="10" verticalAlign="middle">
		<mx:Image smoothBitmapContent="true" source="{catalogoIconosBotoneras.getInstance().programado_mini}" />
		<s:Label styleName="Texto_14_Helvetica" text="Programado" />
	</s:HGroup>  
	<s:HGroup height="100%" gap="10" verticalAlign="middle">
		<mx:Image smoothBitmapContent="true" source="{catalogoIconosBotoneras.getInstance().flete_mini}" />
		<s:Label styleName="Texto_14_Helvetica" text="Flete Express" />
	</s:HGroup>  
	<s:HGroup height="100%" gap="10" verticalAlign="middle">
		<mx:Image smoothBitmapContent="true" source="{catalogoIconosBotoneras.getInstance().stock_mini}" />
		<s:Label styleName="Texto_14_Helvetica" text="Stock" />
	</s:HGroup>  
	<s:HGroup height="100%" gap="10" verticalAlign="middle">
		<mx:Image smoothBitmapContent="true" source="{catalogoIconosBotoneras.getInstance().vincular_stock}" />
		<s:Label styleName="Texto_14_Helvetica" text="Vincular con Stock" />
	</s:HGroup>  
	<s:Label styleName="Texto_14_Helvetica" text="CU: Costo Unitario" />
	<s:Label styleName="Texto_14_Helvetica" text="CT: Costo Total" />
	
</s:HGroup>
