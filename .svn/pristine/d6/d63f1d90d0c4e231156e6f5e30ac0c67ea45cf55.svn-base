		public function PrevioImportacionItemGrid()
		{
			_numPartidasString = "0 Partidas";
		}

		public function get numFila():uint
		{
			return _numFila;
		}

		public function set numFila(value:uint):void
		{
			_numFila = value;
		}

		public function get nombreProveedor():String
		{
			return _nombreProveedor;
		}

		public function set nombreProveedor(value:String):void
		{
			_nombreProveedor = value;
		}

		public function get paisProveedor():String
		{
			return _paisProveedor;
		}

		public function set paisProveedor(value:String):void
		{
			_paisProveedor = value;
		}

		public function get numEnSistema():uint
		{
			return _numEnSistema;
		}

		public function set numEnSistema(value:uint):void
		{
			_numEnSistema = value;
		}

		public function get numFueraSistema():uint
		{
			return _numFueraSistema;
		}

		public function set numFueraSistema(value:uint):void
		{
			_numFueraSistema = value;
		}

		public function get numPartidas():uint
		{
			return _numPartidas;
		}

		public function set numPartidas(value:uint):void
		{
			_numPartidas = value;
			if(value == 1)
				_numPartidasString = value + " Partida";
			else
				_numPartidasString = value + " Partidas";
		}

		public function get numEstandares():uint
		{
			return _numEstandares;
		}

		public function set numEstandares(value:uint):void
		{
			_numEstandares = value;
		}

		public function get numReactivos():uint
		{
			return _numReactivos;
		}

		public function set numReactivos(value:uint):void
		{
			_numReactivos = value;
		}

		public function get numAlopaticos():uint
		{
			return _numAlopaticos;
		}

		public function set numAlopaticos(value:uint):void
		{
			_numAlopaticos = value;
		}

		public function get numPlafest():uint
		{
			return _numPlafest;
		}

		public function set numPlafest(value:uint):void
		{
			_numPlafest = value;
		}

		public function get numPsicotropico():uint
		{
			return _numPsicotropico;
		}

		public function set numPsicotropico(value:uint):void
		{
			_numPsicotropico = value;
		}

		public function get numReqPermiso():uint
