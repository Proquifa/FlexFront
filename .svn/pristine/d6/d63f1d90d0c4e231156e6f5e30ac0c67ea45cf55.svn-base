package vista.modelo
{
	[Bindable]
	public class PrevioImportacionItemGrid
	{
		private var _numFila:uint;
		private var _nombreProveedor:String;
		private var _paisProveedor:String;
		private var _numEnSistema:uint;
		private var _numFueraSistema:uint;
		private var _numPartidas:uint;
		private var _numPartidasString:String;
		private var _numAlopaticos:uint;
		private var _numEstandares:uint;
		private var _numMedicamentos:uint;
		private var _numPublicaciones:uint;
		private var _numReactivos:uint;
