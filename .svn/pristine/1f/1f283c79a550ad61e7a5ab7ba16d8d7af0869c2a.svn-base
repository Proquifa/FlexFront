<?xml version="1.0" encoding="utf-8"?>
<label:TextoAzulEspectral xmlns:fx="http://ns.adobe.com/mxml/2009" 
			 xmlns:s="library://ns.adobe.com/flex/spark" 
			 xmlns:mx="library://ns.adobe.com/flex/mx" xmlns:label="skins.skin.label.*">
	<fx:Script>
		<![CDATA[
			import mx.com.proquifa.proquifanet.rsl.vista.utils.UtilsFecha;
			private var _fecha:Date;

			public function get fecha():Date
			{
				return _fecha;
			}
			public function set fecha(value:Date):void
			{
				_fecha = value;
				this.text = UtilsFecha.formatoFechaDDMMMYYYYHHMM(value);
			}
		]]>
	</fx:Script>
</label:TextoAzulEspectral >
