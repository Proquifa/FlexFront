package mx.com.proquifa.proquifanet.rsl.vista.modelo.comun
{
	[RemoteClass(alias="mx.com.proquifa.proquifanet.modelo.comun.DatosTiempoEntrega")]
	[Bindable]
	public class DatosTiempoEntrega
	{
		private var _dia1:Number;
		private var _dia2:Number;
		private var _fijo:Boolean;
		private var _meses:Number;
		
		
		public function DatosTiempoEntrega()
		{
			_dia1 = 0;
			_dia2 = 0;
			_fijo = false;
			_meses = 0;
		}

		public function get dia1():Number
		{
			return _dia1;
		}

		public function set dia1(value:Number):void
		{
			_dia1 = value;
		}

		public function get dia2():Number
		{
			return _dia2;
		}

		public function set dia2(value:Number):void
		{
			_dia2 = value;
		}

		public function get fijo():Boolean
		{
			return _fijo;
		}

		public function set fijo(value:Boolean):void
		{
			_fijo = value;
		}

		public function get meses():Number
		{
			return _meses;
		}

		public function set meses(value:Number):void
		{
			_meses = value;
		}


	}
}