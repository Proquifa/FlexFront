package modelo.objetos
{
	[Bindable]
	public class itemCatalogo
	{
		
		private var _titulo:String;
		private var _descripcion:String;
		private var _seleccionado:Boolean = false;
		
		public function itemCatalogo(titulo:String="",descripcion:String="",seleccionado:Boolean=false)
		{
			this.titulo = titulo;
			this.descripcion = descripcion;
			this.seleccionado = seleccionado;
		}

		public function get seleccionado():Boolean
		{
			return _seleccionado;
		}

		public function set seleccionado(value:Boolean):void
		{
			_seleccionado = value;
		}

		public function get descripcion():String
		{
			return _descripcion;
		}

		public function set descripcion(value:String):void
		{
			_descripcion = value;
		}

		public function get titulo():String
		{
			return _titulo;
		}

		public function set titulo(value:String):void
		{
			_titulo = value;
		}

	}
}