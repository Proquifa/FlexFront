						accionCargarFactura = "reload";
						var evento:EventoGestorCompras = new EventoGestorCompras(EventoGestorCompras.OBTENER_COMPRAS_POR_PAGAR);
						evento.usuario = DatosGlobales.getInstance()._usuario.usuario;
