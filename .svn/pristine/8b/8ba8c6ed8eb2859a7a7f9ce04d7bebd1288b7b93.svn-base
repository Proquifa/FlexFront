						{
							listaAux[x].conteo = value[y].conteo;
							break;
						}
						else if((listaAux[x].tipo == "registroOCnacionales") && (value[y].pendiente == "confirmarOCnacional"))
						{
							listaAux[x].conteo = value[y].conteo;
