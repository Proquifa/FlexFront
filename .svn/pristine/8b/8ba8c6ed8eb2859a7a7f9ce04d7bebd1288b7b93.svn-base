						evt.ordenDespacho = new OrdenDespacho;
						evt.ordenDespacho.usuario = DatosGlobales.getInstance()._usuario.idEmpleado;
					/*	evt.ordenDespacho.tipoVista = "materiales";*/
						dispatchEvent(evt);
						break;
					}
						
			/*		case "PublDespacho":
