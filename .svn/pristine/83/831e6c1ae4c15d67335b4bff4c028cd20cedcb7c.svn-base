package mx.com.proquifa.proquifanet.rsl.vista.modelo.despachos
{
	import mx.collections.ArrayCollection;

	[RemoteClass(alias="mx.com.proquifa.proquifanet.modelo.despachos.BolsaParaEmbalar")]
	[Bindable]
	public class BolsaParaEmbalar{
		public function BolsaParaEmbalar()
		{
			super();
		}
		private var _noPiezas:Number = 0;
		private var _noPartidas:Number = 0;
		private var _manejo:String = "";
		private var _Serial:String = "";
		private var _idPack:String = "";
		private var _modelo:String = "";
		private var _mostrar:Boolean;
		
		private var _arrayProductos:ArrayCollection = new ArrayCollection;

		public function get noPiezas():Number
		{
			return _noPiezas;
		}

		public function set noPiezas(value:Number):void
		{
			_noPiezas = value;
		}

		public function get noPartidas():Number
		{
			return _noPartidas;
		}

		public function set noPartidas(value:Number):void
		{
			_noPartidas = value;
		}

		public function get manejo():String
		{
			return _manejo;
		}

		public function set manejo(value:String):void
		{
			_manejo = value;
		}

		public function get Serial():String
		{
			return _Serial;
		}

		public function set Serial(value:String):void
		{
			_Serial = value;
		}

		public function get idPack():String
		{
			return _idPack;
		}

		public function set idPack(value:String):void
		{
			_idPack = value;
		}

		public function get arrayProductos():ArrayCollection
		{
			return _arrayProductos;
		}

		public function set arrayProductos(value:ArrayCollection):void
		{
			_arrayProductos = value;
		}

		public function get modelo():String
		{
			return _modelo;
		}

		public function set modelo(value:String):void
		{
			_modelo = value;
		}

		public function get mostrar():Boolean
		{
			return _mostrar;
		}

		public function set mostrar(value:Boolean):void
		{
			_mostrar = value;
		}


	}
}