package mx.com.proquifa.proquifanet.rsl.vista.vistas.compresorVentanas
{
	import mx.collections.ArrayCollection;
	
	import spark.components.Group;
	
	
	public class GroupItemCompresor extends Group
	{
		private var _isMaximized:Boolean = false;
		private var _tipoComponente:String;
		private var _relacionPanelIndex:int;
		private var _indexPanel:uint;
		private var _relacionFlechaIndex:uint;
		
		private var _direccion:String;
		
		public function GroupItemCompresor()
		{
			super();
		}
		
		public function get isMaximized():Boolean
		{
			return _isMaximized;
		}
		
		public function set isMaximized(value:Boolean):void
		{
			_isMaximized = value;
		}
		
		public function get tipoComponente():String
		{
			return _tipoComponente;
		}
		
		public function set tipoComponente(value:String):void
		{
			_tipoComponente = value;
		}
		
		public function get direccion():String
		{
			return _direccion;
		}
		
		public function set direccion(value:String):void
		{
			_direccion = value;
		}
		
		public function get relacionPanelIndex():int
		{
			return _relacionPanelIndex;
		}
		
		public function set relacionPanelIndex(value:int):void
		{
			_relacionPanelIndex = value;
		}
		
		public function get indexPanel():uint
		{
			return _indexPanel;
		}
		
		public function set indexPanel(value:uint):void
		{
			_indexPanel = value;
		}
		
		public function get relacionFlechaIndex():uint
		{
			return _relacionFlechaIndex;
		}
		
		public function set relacionFlechaIndex(value:uint):void
		{
			_relacionFlechaIndex = value;
		}
		
	}
}