<?xml version="1.0" encoding="utf-8"?>
<mx:HBox xmlns:mx="http://www.adobe.com/2006/mxml" width="100%" height="100%" dropShadowVisible="false" borderStyle="none">
	<mx:Script>
		<![CDATA[
			/**
			 * Agrega el componente con un tipo de Búsqueda (rápida o avanzada), removiendo algun otro tipo de Búsqueda existente.
			 */
			public function agregaTipoBusqueda(busqueda:DisplayObject):void{
				if(busqueda == null){
					return;
				}
				this.removeAllChildren();
				this.addChild(busqueda);
			}
		]]>
	</mx:Script>
</mx:HBox>
