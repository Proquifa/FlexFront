<?xml version="1.0" encoding="utf-8"?>
<spark:ScrollBarTrackSkin width="100%" height="5" y="0" visible="false" chromeColor="#6e6e6e"  xmlns:fx="http://ns.adobe.com/mxml/2009" 
						  xmlns:s="library://ns.adobe.com/flex/spark" 
						  xmlns:mx="library://ns.adobe.com/flex/mx" xmlns:spark="mx.skins.spark.*">
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	<fx:Script>
		<![CDATA[
				override protected function initializationComplete():void
				{
					useChromeColor = true;
					super.initializationComplete();
				}
		]]>
	</fx:Script>
	
	<spark:states>
		<s:State name="up" />
		<s:State name="over" />
		<s:State name="down" />
		<s:State name="disabled" />
	</spark:states>
	
	<s:Rect width="100%" height="100%" 
			topLeftRadiusX="3"
			topRightRadiusX="3"
			bottomLeftRadiusX="3"
			bottomRightRadiusX="3">
		<s:fill>
			<s:SolidColor color="#6e6e6e" />
		</s:fill>
		
	</s:Rect>
	
</spark:ScrollBarTrackSkin>
