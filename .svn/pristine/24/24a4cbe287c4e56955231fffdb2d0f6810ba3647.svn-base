package mx.com.proquifa.proquifanet.rsl.vista.modelo.ventas.visita
{
	import mx.collections.ArrayCollection;

	[Bindable]
	public class HorarioCalendario
	{
		public function HorarioCalendario()
		{
		}
		private var _numeroDia:int;
		private var _numeroMes:int;
		private var _mesNombre:String;
		private var _diaNombre:String;
		private var _anio:int;
		private var _siglasDia:String;
		private var _calendario:ArrayCollection;
		private var _totalHoras:int;
		
		public function get totalHoras():int
		{
			return _totalHoras;
		}

		public function set totalHoras(value:int):void
		{
			_totalHoras = value;
		}

		public function get calendario():ArrayCollection
		{
			return _calendario;
		}

		public function set calendario(value:ArrayCollection):void
		{
			_calendario = value;
		}

		public function get siglasDia():String
		{
			return _siglasDia;
		}

		public function set siglasDia(value:String):void
		{
			_siglasDia = value;
		}

		public function get anio():int
		{
			return _anio;
		}

		public function set anio(value:int):void
		{
			_anio = value;
		}

		public function get diaNombre():String
		{
			return _diaNombre;
		}

		public function set diaNombre(value:String):void
		{
			_diaNombre = value;
		}

		public function get mesNombre():String
		{
			return _mesNombre;
		}

		public function set mesNombre(value:String):void
		{
			_mesNombre = value;
		}

		public function get numeroMes():int
		{
			return _numeroMes;
		}

		public function set numeroMes(value:int):void
		{
			_numeroMes = value;
		}

		public function get numeroDia():int
		{
			return _numeroDia;
		}

		public function set numeroDia(value:int):void
		{
			_numeroDia = value;
		}

	}
}