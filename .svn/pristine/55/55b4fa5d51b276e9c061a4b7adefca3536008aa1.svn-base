<?xml version="1.0" encoding="utf-8"?>
<s:VGroup xmlns:fx="http://ns.adobe.com/mxml/2009" 
		  xmlns:s="library://ns.adobe.com/flex/spark" 
		  xmlns:mx="library://ns.adobe.com/flex/mx" 
		  xmlns:label="skins.skin.label.*"
		  xmlns:otros="skins.otros.*" 
		  width="100%" 
		  >
	<fx:Script>
		<![CDATA[
			import skins.catalogos.catalogoIconosBotones;
			import skins.catalogos.catalogoIconosCarteras;
		]]>
	</fx:Script>
	
	<s:HGroup width="100%" paddingTop="20" paddingBottom="10" gap="0" paddingLeft="20" paddingRight="20">
		<s:VGroup width="160" horizontalAlign="right" gap="10">
			<s:HGroup width="100%" >
				<label:Titulo text="FPAc. " fontSize="10" paddingTop="0" width="20%" textAlign="right"/>
				<label:Texto text="Facturación Periódo Actual" fontSize="10" width="80%" textAlign="left"/>
			</s:HGroup>
			<s:HGroup width="100%" >
				<label:Titulo text="Part " fontSize="10" paddingTop="0" width="20%" textAlign="right"/>
				<label:Texto text="Participación" fontSize="10" width="80%" textAlign="left"/>
			</s:HGroup>
		</s:VGroup>
		<s:Spacer width="100%" />
		<s:VGroup width="150" horizontalAlign="right" gap="10">
			<s:HGroup width="100%" >
				<label:Titulo text="OF " fontSize="10" paddingTop="0" width="20%" textAlign="right"/>
				<label:Texto text="Objetivo Fundamental" fontSize="10" width="80%" textAlign="left"/>
			</s:HGroup>
			<s:HGroup width="100%" >
				<label:Titulo text="OD " fontSize="10" paddingTop="0" width="20%" textAlign="right"/>
				<label:Texto text="Objetivo Deseado" fontSize="10" width="80%" textAlign="left"/>
			</s:HGroup>
		</s:VGroup>
		<s:Spacer width="100%" />
		<s:VGroup width="160" horizontalAlign="right" gap="10">
			<s:HGroup width="100%" >
				<label:Titulo text="PV " fontSize="10" paddingTop="0" width="20%" textAlign="right"/>
				<label:Texto text="Proyección de Venta" fontSize="10" width="100%" textAlign="left"/>
			</s:HGroup>
			<s:HGroup verticalAlign="middle" paddingTop="-4" width="100%">
				<s:Image source="{ catalogoIconosBotones.getInstance().indicadorObjetivo}" right="0" width="20%"/>
				<label:Texto text="Objetivo" fontSize="10"  textAlign="left" width="100%"/>
			</s:HGroup>
		</s:VGroup>
		<s:Spacer width="100%" />
		
		<s:VGroup width="150" horizontalAlign="right" gap="10">
			<s:HGroup verticalAlign="middle" paddingTop="-8" width="100%">
				<s:Image source="{ catalogoIconosBotones.getInstance().indicadorPronostico}" right="0" width="20%"/>
				<label:Texto text="Pronóstico de Crecimiento" fontSize="10"  textAlign="left" width="100%"/>
			</s:HGroup>
			<s:HGroup verticalAlign="middle" paddingTop="-8" width="100%">
				<s:Image source="{ catalogoIconosCarteras.getInstance().creador_mini}" right="0" width="20%"/>
				<label:Texto text="Creador" fontSize="10"  textAlign="left" width="100%"/>
			</s:HGroup>
		</s:VGroup>
		<s:Spacer width="100%" />
		
		<s:VGroup width="150" horizontalAlign="right" gap="10">
			<s:HGroup verticalAlign="middle" paddingTop="-8" width="100%">
				<s:Image source="{ catalogoIconosCarteras.getInstance().esac_mini}" right="0" width="20%"/>
				<label:Texto text="ESAC" fontSize="10"  textAlign="left" width="100%"/>
			</s:HGroup>
			<s:HGroup verticalAlign="middle" paddingTop="-8" width="100%">
				<s:Image source="{ catalogoIconosCarteras.getInstance().ev_mini}" right="0" width="20%"/>
				<label:Texto text="EV" fontSize="10"  textAlign="left" width="100%"/>
			</s:HGroup>
		</s:VGroup>
		<s:Spacer width="100%" />
		
		<s:VGroup width="150" horizontalAlign="right" gap="10">
			<s:HGroup verticalAlign="middle" paddingTop="-8" width="100%">
				<s:Image source="{ catalogoIconosCarteras.getInstance().cobrador_mini}" right="0" width="20%"/>
				<label:Texto text="Cobrador" fontSize="10"  textAlign="left" width="100%"/>
			</s:HGroup>
			<s:HGroup verticalAlign="middle" paddingTop="-8" width="100%">
				<s:Image source="{ catalogoIconosCarteras.getInstance().importancia_mini}" right="0" width="20%"/>
				<label:Texto text="Importancia" fontSize="10"  textAlign="left" width="100%"/>
			</s:HGroup>
		</s:VGroup>
		<s:Spacer width="100%" />
		
		<s:VGroup width="150" horizontalAlign="right" gap="10">
			<s:HGroup verticalAlign="middle" paddingTop="-8" width="100%">
				<s:Image source="{ catalogoIconosCarteras.getInstance().dificultad_mini}" right="0" width="20%"/>
				<label:Texto text="Dificultad" fontSize="10"  textAlign="left" width="100%"/>
			</s:HGroup>
			<s:HGroup verticalAlign="middle" paddingTop="-8" width="100%">
				<mx:Image source="{ catalogoIconosCarteras.getInstance().cliente_mini}" right="0" width="20%"/>
				<label:Texto text="Cliente" fontSize="10"  textAlign="left" width="100%"/>
			</s:HGroup>
		</s:VGroup>
		
		
	</s:HGroup>
	
	
</s:VGroup>
