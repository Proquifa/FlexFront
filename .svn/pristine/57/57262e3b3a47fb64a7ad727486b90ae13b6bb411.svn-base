			protected function imgEditar_clickHandler(event:MouseEvent, tipoFlecha:String):void
			{
				if(tipoFlecha == "Adelante")
					indiceSemana = indiceSemana +1;
					
				else if(tipoFlecha == "atras")
					indiceSemana = indiceSemana - 1;
				
				if(indiceSemana == 0)
				{
					atras.enabled = false;
					atras.alpha = 0.4;
				}
					
				else if(indiceSemana == (listaLunes.length -1)){
					Adelante.enabled = false;
					Adelante.alpha = 0.4;
				}
				else{
					atras.enabled = true;
					Adelante.enabled = true;
					atras.alpha = 1;
					Adelante.alpha = 1;
				}
				
				listaLunesPorSemana(listaLunes[indiceSemana]);
				
				
				
				grpLista.removeAllElements();
				
				acomodarSemanasLunes();
				var numeroDias:int = 5;
				var totalHo:Number;
				var totalesHoras:Number = 0;
				var objDiaAuxH:HorarioCalendario;
				var listaHora:VistaListaHora = new VistaListaHora;
				if(visitasVisibles.length > 0){
					
					listaHora.dataProvider = crearCalendarioNuevo();
					grpLista.addElement(listaHora);
					
					for (var j:int = 0; j < numeroDias; j++) 
					{
						totalHo = 0;
						objDiaAuxH = new HorarioCalendario;
						var listaVista:VistaListaSemanaAgendar = new VistaListaSemanaAgendar;
						if(j==0){
							listaVista.dataProvider = objDia1.calendario;
							objDiaAuxH = objDia1;
						}
							
						else if(j==1){
							listaVista.dataProvider = objDia2.calendario;
							objDiaAuxH = objDia2;
						}
							
						else if(j==2)
						{
							listaVista.dataProvider = objDia3.calendario;
							objDiaAuxH = objDia3;
						}
							
						else if(j==3)
						{
							listaVista.dataProvider = objDia4.calendario;	
							objDiaAuxH = objDia4;
						}
							
						else if(j==4)
						{
							listaVista.dataProvider = objDia5.calendario;
							objDiaAuxH = objDia5;
						}
						
						
						listaVista.id = "listaSemana"+j;
						
						
						
						for (var k:int = 0; k < objDiaAuxH.calendario.length ; k++) 
						{
							totalHo = totalHo + objDiaAuxH.calendario[k].totalHoras;
						}
						
						
						if (j == 0)
							hrsTotal1.text = totalHo +" HRS.";
						else if (j == 1)
							hrsTotal2.text = totalHo +" HRS.";
							
						else if ( j == 2)
							hrsTotal3.text = totalHo +" HRS.";
							
						else if ( j == 3)
							hrsTotal4.text = totalHo +" HRS.";
							
						else if ( j == 4)
							hrsTotal5.text = totalHo +" HRS.";
						
						totalesHoras = totalesHoras + totalHo;
						
						
						
						grpLista.addElement(listaVista);
						
					}
					
					lblTotalHoras.text = ""+totalesHoras;
				}
			}
			
			
			
