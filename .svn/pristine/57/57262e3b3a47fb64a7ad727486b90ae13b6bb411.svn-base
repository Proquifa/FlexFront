			private function cargarListaSemana(listaVisitas:ArrayCollection):void
			{       
				grpLista.removeAllElements();
				acomodarCalendariosEnSemana(listaVisitas);
				var numeroDias:int = 5;
				var objDiaAuxH:HorarioCalendario;
				var listaHora:VistaListaHora = new VistaListaHora;
				var totalHo:int;
				var totalesHoras:Number = 0;
				
				if(listaVisitas.length > 0){
					
					listaHora.dataProvider = crearCalendarioNuevo();
					grpLista.addElement(listaHora);
					
					for (var j:int = 0; j < numeroDias; j++) 
					{
						totalHo = 0;
						objDiaAuxH = new HorarioCalendario;
						var listaVista:VistaListaSemanaAgendar = new VistaListaSemanaAgendar;
						
						if(j==0){
							listaVista.dataProvider = objDia1.calendario;
							objDiaAuxH = objDia1;
						}
							
						else if(j==1){
							listaVista.dataProvider = objDia2.calendario;
							objDiaAuxH = objDia2;
						}
							
						else if(j==2)
						{
							listaVista.dataProvider = objDia3.calendario;
							objDiaAuxH = objDia3;
						}
							
						else if(j==3)
						{
							listaVista.dataProvider = objDia4.calendario;	
							objDiaAuxH = objDia4;
						}
							
						else if(j==4)
						{
							listaVista.dataProvider = objDia5.calendario;
							objDiaAuxH = objDia5;
						}
						listaVista.id = "listaSemana"+j;
						
						
						for (var k:int = 0; k < objDiaAuxH.calendario.length; k++) 
						{
							totalHo = totalHo + objDiaAuxH.calendario[k].totalHoras;
						}
						
						
						
						
						grpLista.addElement(listaVista);
					}
					
					lblTotalHoras.text = ""+totalesHoras;
				}
			}
			
			private function cargarListaSprint(listaVisitas:ArrayCollection):void
			{       
				grpListaSprint.removeAllElements();
				headerSprint.removeAllElements();
				footerSprint.removeAllElements();
				
				var objDiaAuxH:HorarioCalendario;
				var listaHora:VistaListaHoraSprint = new VistaListaHoraSprint;
				listaHora.width = 80;
				var j:int = 0;
				
				if(listaVisitas.length > 0){
					var hg1:EncabezadosTituloSprint = new EncabezadosTituloSprint;
					hg1.siglaDia = "TIEMPO";
					hg1.width = 80;
					listaHora.dataProvider = crearCalendarioNuevo();
					
					var hgP:VGroup = new VGroup;
					hgP.percentHeight = 100;
					hgP.width = 80;
					hgP.gap = 0;
					hgP.horizontalAlign = "center";
					
					var pieVisitarT:PieContadorHoras = new PieContadorHoras;
					pieVisitarT.tipo = "tituloHoras";
					
					var pieAsignadoT:PieContadorHoras = new PieContadorHoras;
					pieAsignadoT.tipo = "tituloTotal";
					
					var grpTotalesT:VGroup = new VGroup();
					grpTotalesT.percentHeight = 100;
					grpTotalesT.width = 80;
					grpTotalesT.gap = 0;
					grpTotalesT.addElement(pieVisitarT);
					grpTotalesT.addElement(pieAsignadoT);

					headerSprint.addElement(hg1);
					hgP.addElement(listaHora);
					footerSprint.addElement(grpTotalesT);
					
					grpListaSprint.addElement(hgP);
					
					for each (var temp:HorarioCalendario in listaVisitas) 
					{
						var listaVista:VistaListaSprintAgendar = new VistaListaSprintAgendar;
						listaVista.percentWidth = 100;
						listaVista.id = "listaSprint"+j;
						j++;
						
						
						var hg:EncabezadosTituloSprint = new EncabezadosTituloSprint;
						hg.siglaDia = temp.diaNombre.charAt(0);
						hg.numeroDia = temp.numeroDia;
						
						var hgP:VGroup = new VGroup;
						hgP.percentHeight = hgP.percentWidth = 100;
						hgP.gap = 0;
						
						var pieVisitarT:PieContadorHoras = new PieContadorHoras;
						pieVisitarT.tipo = "horas";
						pieVisitarT.label = "0";
						
						var pieAsignadoT:PieContadorHoras = new PieContadorHoras;
						pieAsignadoT.tipo = "total";
						pieAsignadoT.label = "9";
						
						var grpTotales:VGroup = new VGroup();
						grpTotales.percentHeight = 100;
						grpTotales.percentWidth = 100;
						grpTotales.gap = 0;
						grpTotales.addElement(pieVisitarT);
						grpTotales.addElement(pieAsignadoT);
						
						headerSprint.addElement(hg);
						hgP.addElement(listaVista);
						footerSprint.addElement(grpTotales);
						
						listaVista.dataProvider = temp.calendario;
						grpListaSprint.addElement(hgP);
					}
					
					lblTotalHorasSprint.text = "DE "+listaVisitas.length * 9+" HRS.";
				}
			}
			
			protected function scroll_mouseWheelChangingHandler(event:FlexMouseEvent):void
			{
				var move:Number = scroll.viewport.verticalScrollPosition;
				if(event.delta < 0)
					scroll.viewport.verticalScrollPosition = move+10;
				else
					scroll.viewport.verticalScrollPosition = move-10;
			}
			
			protected function scroll_mouseWheelChangingHandlerSprint(event:FlexMouseEvent):void
			{
				var move:Number = scroll.viewport.verticalScrollPosition;
				event.delta = event.delta > 0 ? 20 : -20;
			}
			
