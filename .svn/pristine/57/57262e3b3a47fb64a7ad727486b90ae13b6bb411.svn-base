			private function cargarItemsVisita(listaFechas:ArrayCollection):void
			{
				
				
				for each (var fechas:HorarioCalendario in listaFechas) 
				{
					fechas.calendario = new ArrayCollection;
					fechas.totalHoras = 0;
					
					var listaDia:ArrayCollection = new ArrayCollection;
					for (var i:int = 8; i <= 18; i++) 
					{
						var obj:HorarioTiempo = new HorarioTiempo;
						obj.hora = "";
						obj.descripcion = "";
						obj.horario = "";
						obj.visitaCliente = new VisitaCliente;//QUITAR POSIBLEMENTE
						obj.totalHoras = 0;
						obj.hora = i+":00";
						
						listaDia.addItem(obj);
						
						if(i < 18)
						{
							var obj:HorarioTiempo = new HorarioTiempo;
							obj.hora = "";
							obj.descripcion = "";
							obj.horario = "";
							obj.visitaCliente = new VisitaCliente;
							obj.totalHoras = 0;
							obj.hora = i+":30";
							
							listaDia.addItem(obj);
						}
					}
					
					fechas.calendario = listaDia;
				}
				
				cargarListaDia(listaFechas);
				
				cargarListaSemana(listaFechas);
				
				cargarListaSprint(listaFechas);
				
				visitasVisibles = listaFechas;
			}
			
			public function acomodarVisita(visita:VisitaCliente):void
			{
				var horafinal:int = 0;
				var horaFaux:int = 0;
				var horaIaux:int = 0;
				var auxContador:int = 0;
				var horarioDisponible:Boolean;
				var numeroHora:int;
				var objAux:Object = new Object();
				for each (var vistaTemp:Object in visitasVisibles) 
				{
					if(visita.fechaVisita.date == vistaTemp.numeroDia && visita.fechaVisita.month == vistaTemp.numeroMes){
						
						var horaActual:String = visita.fechaVInicioHora
							+":"+regresarCadenaTiempo(visita.fechaVInicioMinuto);
						
						for (var k:int = 0; k < vistaTemp.calendario.length; k++) 
						{
							if( vistaTemp.calendario[k].hora == horaActual )
							{
								var objTemp:Object = vistaTemp.calendario[k];
								numeroHora = k;
								break;
							}
						}
						/*var objTemp:Object = vistaTemp.calendario[visita.fechaVInicioHora];*/
						
						var hora:String = regresarCadenaTiempo(visita.fechaVInicioHora)
							+":"+regresarCadenaTiempo(visita.fechaVInicioMinuto)+" - "
							+regresarCadenaTiempo(visita.fechaVFinHora)+":"
							+regresarCadenaTiempo(visita.fechaVFinMinuto);
						
						var horaFinal:String = visita.fechaVFinHora +":"+regresarCadenaTiempo(visita.fechaVFinMinuto);
						
						
						objTemp.totalHoras = 0.5;
						objTemp.horario = hora;
						objTemp.descripcion = visita.nombreCliente;
						objTemp.visitaCliente = ObjectUtil.copy(visita) as VisitaCliente;
						
						/*se le resta 1 a la hora final*/
						horaIaux = visita.fechaVInicioHora;
						horafinal = visita.fechaVFinHora -1;
						
						vistaTemp.calendario.setItemAt(objTemp,numeroHora);
						
						
						
						for (var j:int = numeroHora+1; j < vistaTemp.calendario.length ; j++) 
						{
							if( vistaTemp.calendario[j].hora != horaFinal)
							{
								objAux = vistaTemp.calendario.getItemAt(j);
								objAux.visitaCliente = ObjectUtil.copy(visita) as VisitaCliente;
								objAux.totalHoras = 0.5;
								objAux.descripcion = "objetoVacio"; 
								vistaTemp.calendario.setItemAt(objAux,j);
							}
							else if( vistaTemp.calendario[j].hora == horaFinal)
							{
								break;
							}
						}
						
						
						
						var totalHoras:Number;
						for each (var i:Object in vistaTemp.calendario) 
						{
							totalHoras += i.totalHoras;
							
						}
						vistaTemp.totalHoras = totalHoras;
						
						break;
					}
				}
				asignarTotales();
			}
			
			
			public function verificarHorarioDisponible(visitaAG:VisitaCliente):Boolean{
				var horarioDisponible:Boolean;
				var numeroHora:int;
				for each (var vistaTemp:Object in visitasVisibles) 
				{
					if(visitaAG.fechaVisita.date == vistaTemp.numeroDia && visitaAG.fechaVisita.month == vistaTemp.numeroMes){
						
						var horaActual:String = visitaAG.fechaVInicioHora
							+":"+regresarCadenaTiempo(visitaAG.fechaVInicioMinuto);
						
						var horaFinal:String = visitaAG.fechaVFinHora +":"+regresarCadenaTiempo(visitaAG.fechaVFinMinuto);
						
						for (var k:int = 0; k < vistaTemp.calendario.length; k++) 
						{
							if( vistaTemp.calendario[k].hora == horaActual )
							{
								var objTemp:Object = vistaTemp.calendario[k];
								numeroHora = k;
								break;
							}
						}
						
						for (var h:int = numeroHora ; h <= vistaTemp.calendario.length; h++) 
						{
							if(vistaTemp.calendario[h].horario == "" && vistaTemp.calendario[h].descripcion == ""){
								horarioDisponible = true;
								
								if( vistaTemp.calendario[h].hora == horaFinal)
								{
									break;
								}
							}
							else if(vistaTemp.calendario[h].horario != "" && vistaTemp.calendario[h].descripcion != "" && vistaTemp.calendario[h].hora == horaFinal){
								horarioDisponible = true;
								
								if( vistaTemp.calendario[h].hora == horaFinal)
								{
									break;
								}
							}
							else {
								horarioDisponible = false;
								break;
							}
							
						}
						
						break;
					}
				}
				
				return horarioDisponible;
			}
			
			
			
			public function limpiarvisitaDeHorarioAnterior(visita:VisitaCliente):void{
				var horafinal:int = 0;
				var horaFaux:int = 0;
				var horaIaux:int = 0;
				var auxContador:int = 0;
				var horarioDisponible:Boolean;
				var numeroHora:int;
				var objAux:Object = new Object();
				for each (var vistaTemp:Object in visitasVisibles) 
				{
					if(visita.fechaVisita.date == vistaTemp.numeroDia && visita.fechaVisita.month == vistaTemp.numeroMes){
						
						var horaActual:String = visita.fechaVInicioHora
							+":"+regresarCadenaTiempo(visita.fechaVInicioMinuto);
						
						for (var k:int = 0; k < vistaTemp.calendario.length; k++) 
						{
							if( vistaTemp.calendario[k].hora == horaActual )
							{
								var objTemp:Object = vistaTemp.calendario[k];
								numeroHora = k;
								break;
							}
						}
						
						
						var hora:String = "";
						
						var horaFinal:String = visita.fechaVFinHora +":"+regresarCadenaTiempo(visita.fechaVFinMinuto);
						
						
						objTemp.totalHoras = 0;
						objTemp.horario = hora;
						objTemp.descripcion = "";
						objTemp.visitaCliente = ObjectUtil.copy(visita) as VisitaCliente;
						
						
						vistaTemp.calendario.setItemAt(objTemp,numeroHora);
						
						
						
						for (var j:int = numeroHora+1; j < vistaTemp.calendario.length ; j++) 
						{
							if( vistaTemp.calendario[j].hora != horaFinal)
							{
								objAux = vistaTemp.calendario.getItemAt(j);
								objAux.visitaCliente = null;
								objAux.totalHoras = 0;
								objAux.descripcion = ""; 
								vistaTemp.calendario.setItemAt(objAux,j);
							}
								
							else if( vistaTemp.calendario[j].hora == horaFinal)
							{
								break;
							}
						}
						
						
						
						var totalHoras:Number;
						for each (var i:Object in vistaTemp.calendario) 
						{
							totalHoras += i.totalHoras;
							
						}
						vistaTemp.totalHoras = totalHoras;
						
						break;
					}
				}
				
				
				
			}
			
			
			private function regresarCadenaTiempo(tiempo:int):String
			{
				var tiempoString:String = "";
				if(tiempo < 10)
					tiempoString = "0"+tiempo;
				else
					tiempoString = ""+tiempo
				return tiempoString;
			}
			
			public function asignarTotales():void
			{
				var total:Number;
				var totalesHoras:Number = 0;
				var objDiaAuxH:HorarioCalendario;
				if(visitasVisibles.length > 0){
					var componente:String = "";
					for (var i:int = 0; i < 5 ; i++) 
					{
						total = 0;
						objDiaAuxH = new HorarioCalendario;
						
						if(i ==0)
							objDiaAuxH = objDia1;
							
							
						else if( i ==1)
							objDiaAuxH = objDia2;
							
							
						else if(i==2)
							objDiaAuxH = objDia3;
							
							
						else if(i==3)
							objDiaAuxH = objDia4;
							
							
						else if(i==4)
							objDiaAuxH = objDia5;
						
						
						
						for (var k:int = 0; k < objDiaAuxH.calendario.length; k++) 
						{
							total = total + objDiaAuxH.calendario[k].totalHoras;
						}
						
						if (i == 0)
							hrsTotal1.text = total +" HRS.";
						else if (i == 1)
							hrsTotal2.text = total +" HRS.";
							
						else if ( i == 2)
							hrsTotal3.text = total +" HRS.";
							
						else if ( i == 3)
							hrsTotal4.text = total +" HRS.";
							
						else if ( i == 4)
							hrsTotal5.text = total +" HRS.";
						
						totalesHoras = totalesHoras + total;
					}
					
					var today:Date = new Date();
					var totalHorasDia = 0;
					var totalHorasSprint = 0;
					var indicaFoot:int = 1;
					
					for each (var calendario:HorarioCalendario in visitasVisibles) 
					{
						var totalHoras:Number = 0;
						for each (var horario:HorarioTiempo in calendario.calendario) 
						{
							totalHoras+= horario.totalHoras;
						}
						totalHorasSprint += totalHoras;
						
						if(footerSprint != null &&
							(footerSprint.getElementAt(indicaFoot) as VGroup) != null &&
							((footerSprint.getElementAt(indicaFoot) as VGroup).getElementAt(0) as PieContadorHoras) != null
						){
							((footerSprint.getElementAt(indicaFoot) as VGroup).getElementAt(0) as PieContadorHoras).label = ""+totalHoras;
						}
						
						if(today.getDate() == calendario.numeroDia && today.getMonth() == calendario.numeroMes && today.getFullYear() == calendario.anio){
							totalHorasDia = totalHoras;
						}
						indicaFoot++;
					}
					
					lblContadorHorasSprint.text = totalHorasSprint+"";
					
					lblHoraDia.text = totalHorasDia+" HRS.";
					lblTotalHorasDia.text = ""+totalHorasDia;
					
				}
				
				lblTotalHoras.text = ""+totalesHoras;
			}
			
			[Bindable]private var calendarioVacio:ArrayCollection;
			private function crearDiaVacioActual():ArrayCollection
			{
				if(!calendarioVacio){
					var listaDia:ArrayCollection = new ArrayCollection;
					for (var i:int = 0; i <= 18; i++) 
					{
						var obj:HorarioTiempo = new HorarioTiempo;
						obj.hora = "";
						obj.descripcion = "";
						obj.horario = "";
						obj.visitaCliente = new VisitaCliente;
						obj.totalHoras = 0;
						obj.hora = i+":00";
						
						listaDia.addItem(obj);
						if(i < 18)
						{
							var obj:HorarioTiempo = new HorarioTiempo;
							obj.hora = "";
							obj.descripcion = "";
							obj.horario = "";
							obj.visitaCliente = new VisitaCliente;
							obj.totalHoras = 0;
							obj.hora = i+":30";
							
							listaDia.addItem(obj);
						}
					}
					calendarioVacio = listaDia;
				}
				
				return calendarioVacio;
			}
			private function crearCalendarioNuevo():ArrayCollection
			{
				var listaDia:ArrayCollection = new ArrayCollection;
				for (var i:int = 8; i <= 18; i++) 
				{
					var obj:HorarioTiempo = new HorarioTiempo;
					obj.hora = "";
					obj.descripcion = "";
