package mx.com.proquifa.proquifanet.rsl.vista.modelo.ventas.visita
{
	
	[Bindable]
	public class HorarioTiempo
	{
		public function HorarioTiempo()
		{
		}
		private var _hora:String;
		private var _descripcion:String;
		private var _horario:String;
		private var _visitaCliente:VisitaCliente;
		private var _totalHoras:int;

		public function get totalHoras():int
		{
			return _totalHoras;
		}

		public function set totalHoras(value:int):void
		{
			_totalHoras = value;
		}

		public function get visitaCliente():VisitaCliente
		{
			return _visitaCliente;
		}

		public function set visitaCliente(value:VisitaCliente):void
		{
			_visitaCliente = value;
		}

		public function get horario():String
		{
			return _horario;
		}

		public function set horario(value:String):void
		{
			_horario = value;
		}

		public function get descripcion():String
		{
			return _descripcion;
		}

		public function set descripcion(value:String):void
		{
			_descripcion = value;
		}

		public function get hora():String
		{
			return _hora;
		}

		public function set hora(value:String):void
		{
			_hora = value;
		}

	}
}