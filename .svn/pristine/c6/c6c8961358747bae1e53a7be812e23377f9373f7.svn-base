			override protected function updateDisplayList(unscaledWidth:Number, unscaledHeight:Number):void{
				super.updateDisplayList(unscaledWidth,unscaledHeight);
				trace("Width: "+unscaledWidth);
				if(unscaledWidth <= 1430){
					_fontsize=14;
					grpObjActual.percentWidth=100;
					grpObjAnterior.percentWidth=100;
					_spaceheight=16;
					/*grpObjActual.width=172;
					grpObjAnterior.width=172;*/
					
				}
				else if(unscaledWidth > 1430){
					_fontsize=16;
					grpObjActual.width=300;
					grpObjAnterior.width=300;
					_spaceheight=15;
				}
			}
			
			public function iniciarVista():void
			{
				limpiarContenido();
				mostrarTxtInputs(false);
				grpGuardarCancelar.includeInLayout = grpGuardarCancelar.visible = false;
				btnEditar.includeInLayout = btnEditar.visible = true;
				
				var cambio:Boolean = false;
				for (var j:Number=0; j < empleado.roles.length; j++)
				{
					if (empleado.roles[j]=="CatCliente_Master")
					{
						cambio = true;
						break;
					}
				}
				
				if (cambio)
				{
					btnEditar.enabled = true;
				}
				else
				{
					btnEditar.enabled = false;
				}
				
				dispatchEvent(new EventoCatClientes_General(EventoCatClientes_General.OBTENER_OBJETIVOS_DE_CRECIMIENTO));
				dispatchEvent(new EventoCatClientes_General(EventoCatClientes_General.OBTENER_OBJETIVOS_NIVEL_INGRESO_ANTERIORES));
				dispatchEvent(new EventoCatClientes_General(EventoCatClientes_General.OBTENER_OBJETIVOS_NIVEL_INGRESO_ACTUALES));
			}
			
			protected function btnEditar_clickHandler(event:MouseEvent):void
			{
				mostrarTxtInputs(true);
				seEstaEditando=true;
				btnEditar.includeInLayout = btnEditar.visible = false;
				grpGuardarCancelar.includeInLayout = grpGuardarCancelar.visible = true;
			}
			
			protected function btnGuardarObjetivosCrecimiento_clickHandler(event:MouseEvent):void
			{
				for (var i:int = 0; i < objCrecimiento.length; i++) 
				{
					var currentItem:ObjetivoCrecimiento = objCrecimiento[i] as ObjetivoCrecimiento;
					switch(currentItem.nivelIngreso)
					{
						case "AAplus":
						{
							currentItem.porcentajeAnual = Number(txtAAplus_deseado.text);
							currentItem.porcentajeAnualFundamental = Number(txtAAplus_fundamental.text);
							break;
						}
						case "AA":
						{
							currentItem.porcentajeAnual = Number(txtAA_deseado.text);
							currentItem.porcentajeAnualFundamental = Number(txtAA_fundamental.text);
							break;
						}
						case "AM":
						{
							currentItem.porcentajeAnual = Number(txtAM_deseado.text);
							currentItem.porcentajeAnualFundamental = Number(txtAM_fundamental.text);
							break;
						}
						case "AB":
						{
							currentItem.porcentajeAnual = Number(txtAB_deseado.text);
							currentItem.porcentajeAnualFundamental = Number(txtAB_fundamental.text);
							break;
						}
						case "MA":
						{
							currentItem.porcentajeAnual = Number(txtMA_deseado.text);
							currentItem.porcentajeAnualFundamental = Number(txtMA_fundamental.text);
							break;
						}
						case "MM":
						{
							currentItem.porcentajeAnual = Number(txtMM_deseado.text);
							currentItem.porcentajeAnualFundamental = Number(txtMM_fundamental.text);
							break;
						}
						case "MB":
						{
							currentItem.porcentajeAnual = Number(txtMB_deseado.text);
							currentItem.porcentajeAnualFundamental = Number(txtMB_fundamental.text);
							break;
						}
						case "Bajo":
						{	
							currentItem.porcentajeAnual = Number(txtBAJO_deseado.text);
							currentItem.porcentajeAnualFundamental = Number(txtBAJO_fundamental.text);
							break;
						}
						case "Distribuidor":
						{	
							currentItem.porcentajeAnual = Number(txtDistribuidor_deseado.text);
							currentItem.porcentajeAnualFundamental = Number(txtDistribuidor_fundamental.text);
							break;
						}
					}
				} 
				
				if(validarTxtInputs())
				{
					var guardarObjetivos:EventoCatClientes_General = new EventoCatClientes_General(EventoCatClientes_General.ACTUALIZAR_OBJETIVOS_DE_CRECIMIENTO_POR_NIVELES_DE_INGRESO);
					guardarObjetivos.listaObjetivosCrecimiento = objCrecimiento;
					guardarObjetivos.usuarioLogueado = empleado.usuario;
					guardarObjetivos.ventana = "CatClientes_ConfigObjetivos"
					dispatchEvent(guardarObjetivos);
					obtenerValoresMontos();
				}
				else
				{
					alertaSingleton.show("Algunos de los datos que se han introducido no son válidos, favor de verificarlos.",catalogoAlertas.ADV_informacionIncompleta);
				}
			}
			
			protected function btnCancelarGuardadoObjetivosCrecimiento_clickHandler(event:MouseEvent):void
			{
				limpiarContenido();
				mostrarTxtInputs(false);
				btnEditar.includeInLayout = btnEditar.visible = true;
