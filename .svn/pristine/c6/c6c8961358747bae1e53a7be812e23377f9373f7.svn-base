			public function set recibeConfirmacionDeActualizarListaObjetivosCreciNI($confirmacion:Boolean):void
			{
				if($confirmacion)
				{
					iniciarVista();
					var $evt:EventoCatClientes_General = new EventoCatClientes_General( EventoCatClientes_General.OBTENER_CLIENTES);
					$evt.user = DatosGlobales.getInstance()._usuario;
					$evt.habilitado = 1;
					dispatchEvent($evt);
				}
			}
			
			private function mostrarTxtInputs($visible:Boolean):void
			{
				if($visible)
				{
					lblNA1.width=83;
					lblNA2.width=83;
				}
				else
				{
					lblNA1.width=48;
					lblNA2.width=48;
				} 
				
				txtAAplus_deseado.visible = txtAAplus_deseado.includeInLayout = $visible;
				txtAA_deseado.visible = txtAA_deseado.includeInLayout = $visible;
				txtAM_deseado.visible = txtAM_deseado.includeInLayout = $visible;
				txtAB_deseado.visible = txtAB_deseado.includeInLayout = $visible;
				txtMA_deseado.visible = txtMA_deseado.includeInLayout = $visible;
				txtMM_deseado.visible = txtMM_deseado.includeInLayout = $visible;
				txtMB_deseado.visible = txtMB_deseado.includeInLayout = $visible;
