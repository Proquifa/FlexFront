			private function obtenerValoresMontos():void
			{
				if (aaPlus_Ant && txtAAplus_deseado.text != "")
				{
					
					var _valAAPlus_deseado:Number = aaPlus_Ant.montoAnual * ((Number(txtAAplus_deseado.text)/ 100)+1); 
					var _valAA_deseado:Number = aa_Ant.montoAnual * ((Number(txtAA_deseado.text)/ 100)+1); 
					var _valAM_deseado:Number = am_Ant.montoAnual * ((Number(txtAM_deseado.text)/ 100)+1); 
					var _valAB_deseado:Number = ab_Ant.montoAnual * ((Number(txtAB_deseado.text)/ 100)+1); 
					var _valMA_deseado:Number = ma_Ant.montoAnual * ((Number(txtMA_deseado.text)/ 100)+1); 
					var _valMM_deseado:Number = mm_Ant.montoAnual * ((Number(txtMM_deseado.text)/ 100)+1); 
					var _valMB_deseado:Number = mb_Ant.montoAnual * ((Number(txtMB_deseado.text)/ 100)+1); 
					var _valBajo_deseado:Number = bajo_Ant.montoAnual * ((Number(txtBAJO_deseado.text)/ 100)+1); 
					var _valDistri_deseado:Number = distribuidor_Ant.montoAnual * ((Number(txtDistribuidor_deseado.text)/ 100)+1); 
					var _totalDeseado:Number = _valAAPlus_deseado + _valAA_deseado + _valAB_deseado + _valAM_deseado + _valBajo_deseado + _valDistri_deseado + _valMA_deseado + _valMB_deseado + _valMM_deseado;
					var _promedioPorcentajes:Number =   ((_totalDeseado/total_Ant)-1)*100;
					
					
					lblAAplusValor_Deseado.text = "$ " + UtilsFormatosNumericos.precisionADosDecimalesConComas(_valAAPlus_deseado);
					lblAAValor_Deseado.text = "$ " + UtilsFormatosNumericos.precisionADosDecimalesConComas(_valAA_deseado);
					lblAMValor_Deseado.text = "$ " + UtilsFormatosNumericos.precisionADosDecimalesConComas(_valAM_deseado);
					lblABValor_Deseado.text = "$ " + UtilsFormatosNumericos.precisionADosDecimalesConComas(_valAB_deseado);
					lblMAValor_Deseado.text = "$ " + UtilsFormatosNumericos.precisionADosDecimalesConComas(_valMA_deseado);
					lblMMValor_Deseado.text = "$ " + UtilsFormatosNumericos.precisionADosDecimalesConComas(_valMM_deseado);
					lblMBValor_Deseado.text = "$ " + UtilsFormatosNumericos.precisionADosDecimalesConComas(_valMB_deseado);
					lblBAJOValor_Deseado.text = "$ " + UtilsFormatosNumericos.precisionADosDecimalesConComas(_valBajo_deseado);
					lblDistribuidorValor_Deseado.text = "$ " + UtilsFormatosNumericos.precisionADosDecimalesConComas(_valDistri_deseado);
					lblPorcientos_deseado.text = UtilsFormatosNumericos.precisionADosDecimales(_promedioPorcentajes) + " %";
					lblMontos_Deseado.text = "TOTAL: $" + UtilsFormatosNumericos.precisionADosDecimalesConComas(_totalDeseado);
					
					var _valAAPlus_fundamental:Number = aaPlus_Ant.montoAnual * ((Number(txtAAplus_fundamental.text)/ 100)+1); 
					var _valAA_fundamental:Number = aa_Ant.montoAnual * ((Number(txtAA_fundamental.text)/ 100)+1); 
					var _valAM_fundamental:Number = am_Ant.montoAnual * ((Number(txtAM_fundamental.text)/ 100)+1); 
					var _valAB_fundamental:Number = ab_Ant.montoAnual * ((Number(txtAB_fundamental.text)/ 100)+1); 
					var _valMA_fundamental:Number = ma_Ant.montoAnual * ((Number(txtMA_fundamental.text)/ 100)+1); 
					var _valMM_fundamental:Number = mm_Ant.montoAnual * ((Number(txtMM_fundamental.text)/ 100)+1); 
					var _valMB_fundamental:Number = mb_Ant.montoAnual * ((Number(txtMB_fundamental.text)/ 100)+1); 
					var _valBajo_fundamental:Number = bajo_Ant.montoAnual * ((Number(txtBAJO_fundamental.text)/ 100)+1); 
					var _valDistri_fundamental:Number = distribuidor_Ant.montoAnual * ((Number(txtDistribuidor_fundamental.text)/ 100)+1); 
					var _totalDeseado_fundamental:Number = _valAAPlus_fundamental + _valAA_fundamental + _valAB_fundamental + _valAM_fundamental + _valBajo_fundamental + _valDistri_fundamental + _valMA_fundamental + _valMB_fundamental + _valMM_fundamental;
					var _promedioPorcentajes_fundamental:Number =  ((_totalDeseado_fundamental/total_Ant)-1)*100;
					
					lblAAplusValor_fundamental.text = "$ " + UtilsFormatosNumericos.precisionADosDecimalesConComas(_valAAPlus_fundamental);
					lblAAValor_fundamental.text = "$ " + UtilsFormatosNumericos.precisionADosDecimalesConComas(_valAA_fundamental);
					lblAMValor_fundamental.text = "$ " + UtilsFormatosNumericos.precisionADosDecimalesConComas(_valAM_fundamental);
					lblABValor_fundamental.text = "$ " + UtilsFormatosNumericos.precisionADosDecimalesConComas(_valAB_fundamental);
					lblMAValor_fundamental.text = "$ " + UtilsFormatosNumericos.precisionADosDecimalesConComas(_valMA_fundamental);
					lblMMValor_fundamental.text = "$ " + UtilsFormatosNumericos.precisionADosDecimalesConComas(_valMM_fundamental);
					lblMBValor_fundamental.text = "$ " + UtilsFormatosNumericos.precisionADosDecimalesConComas(_valMB_fundamental);
					lblBAJOValor_fundamental.text = "$ " + UtilsFormatosNumericos.precisionADosDecimalesConComas(_valBajo_fundamental);
					lblDistribuidorValor_fundamental.text = "$ " + UtilsFormatosNumericos.precisionADosDecimalesConComas(_valDistri_fundamental);
					lblPorcientos_fundamental.text = UtilsFormatosNumericos.precisionADosDecimales(_promedioPorcentajes_fundamental) + " %";
					lblMontos_fundamental.text = "TOTAL: $" + UtilsFormatosNumericos.precisionADosDecimalesConComas(_totalDeseado_fundamental);
				}
			}	
			
			
			protected function restablecerValores($txtInput:IVisualElement):void
			{
				var error:ValidationResultEvent;
				if($txtInput != null && $txtInput is TextInput)
				{
					var currentTxtInput:TextInput = $txtInput as TextInput;
					validatorPorcentajePositivo.listener = currentTxtInput;
					error = validatorPorcentajePositivo.validate(currentTxtInput.text);
				}
				obtenerValoresMontos();
			}
			
