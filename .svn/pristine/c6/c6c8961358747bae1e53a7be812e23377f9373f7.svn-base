			private function settearContenido($listaObjetivos:ArrayCollection):void
			{
				if($listaObjetivos != null)
				{
					for (var i:int = 0; i < $listaObjetivos.length; i++) 
					{
						var currentItem:ObjetivoCrecimiento = $listaObjetivos[i] as ObjetivoCrecimiento;
						switch(currentItem.nivelIngreso)
						{
							case "AAplus":
							{
								txtAAplus_deseado.text =(currentItem.porcentajeAnual).toString();
								txtAAplus_fundamental.text = (currentItem.porcentajeAnualFundamental).toString();
								break;
							}
							case "AA":
							{
								txtAA_deseado.text=(currentItem.porcentajeAnual).toString();
								txtAA_fundamental.text = (currentItem.porcentajeAnualFundamental).toString();
								break;
							}
							case "AM":
							{
								txtAM_deseado.text=(currentItem.porcentajeAnual).toString();
								txtAM_fundamental.text = (currentItem.porcentajeAnualFundamental).toString();
								break;
							}
							case "AB":
							{
								txtAB_deseado.text=(currentItem.porcentajeAnual).toString();
								txtAB_fundamental.text = (currentItem.porcentajeAnualFundamental).toString();
								break;
							}
							case "MA":
							{
								txtMA_deseado.text=(currentItem.porcentajeAnual).toString();
								txtMA_fundamental.text = (currentItem.porcentajeAnualFundamental).toString();
								break;
							}
							case "MM":
							{
								txtMM_deseado.text=(currentItem.porcentajeAnual).toString();
								txtMM_fundamental.text = (currentItem.porcentajeAnualFundamental).toString();
								break;
							}
							case "MB":
							{
								txtMB_deseado.text=(currentItem.porcentajeAnual).toString();
								txtMB_fundamental.text = (currentItem.porcentajeAnualFundamental).toString();
								break;
							}
							case "Bajo":
							{	
								txtBAJO_deseado.text=(currentItem.porcentajeAnual).toString();
								txtBAJO_fundamental.text = (currentItem.porcentajeAnualFundamental).toString();
								break;
							}
							case "Distribuidor":
							{	
								txtDistribuidor_deseado.text=(currentItem.porcentajeAnual).toString();
								txtDistribuidor_fundamental.text = (currentItem.porcentajeAnualFundamental).toString();
