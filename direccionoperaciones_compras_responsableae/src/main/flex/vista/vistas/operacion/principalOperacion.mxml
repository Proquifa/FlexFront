					popUpPieza.recibeInformacion(pieza );
					popUpPieza.addEventListener("cancelarPopUP",cerrarPopCotizacion);
					popUpPieza.addEventListener("aceptarCambios",aceptarPopCotizacion);
					
				}else{
					
					PopUpManager.addPopUp( popUpPieza , this, true );
					
				}
			}
			
